        <surface>
            <zone type="main" xml:id="mainzone">
                <line xml:id="la" rend="indent-20"><handShift new="#jo_t"/>Ja auf einmal wird es
                        düſter<handShift new="#g_t"/>,</line>
                <line xml:id="lb" rend="indent-20"><handShift new="#jo_t"/>Ohne Glanz entſchwebt
                    der Nebel</line>
                <line xml:id="lc" rend="indent-20">Dunk<f:overw>
                        <f:under>l</f:under>
                        <f:over>e</f:over>
                    </f:overw>lgrau und Mauergrau<handShift new="#g_t"/>.</line>
                <line xml:id="ld" rend="indent-20"><handShift new="#jo_t"/>Mauern ſtellen ſich
                    dem Blicke</line>
                <line f:left="#seg_r" xml:id="le" type="inter"><handShift new="#g_t"/>r</line>
                <line xml:id="lf" rend="indent-20"><handShift new="#jo_t"/>Freyem Blicke
                        ſtar<anchor xml:id="seg_r"/><mod rend="strikethrough" hand="#g_t">k</mod> entgegen<handShift new="#g_t"/>;</line>
                <line xml:id="lg" rend="indent-20"><handShift new="#jo_t"/>Iſts ein Hof<handShift new="#g_t"/>? <handShift new="#jo_t"/>iſts tiefe Grube<handShift new="#g_t"/><retrace hand="#g_t">?</retrace></line>
                <line xml:id="lh" rend="indent-20"><handShift new="#jo_t"/>Schauerlich in jedem
                        Falle<handShift new="#g_t"/>!</line>
                <line xml:id="li" rend="indent-20"><handShift new="#jo_t"/>Schweſtern ach! wir
                    ſind gefangen</line>
                <line xml:id="lj" rend="indent-20">So gefangen wie nur je<retrace hand="#g_t">.</retrace></line>
                <line xml:id="lk" rend="centered"><abbr><hi rend="underline">Chorf.</hi></abbr></line>
                <line xml:id="ll">Vorſchnell und thörig, ächt wahrhaftes Weibsgebild<handShift new="#g_bl"/>!</line>
                <line xml:id="lm"><handShift new="#jo_t"/>Vom Augenblick abhängig<handShift new="#g_bl"/>, <handShift new="#jo_t"/>Sklaven jeder Witterung</line>
                <line xml:id="ln">Des Glücks und Unglücks<handShift new="#g_bl"/>, <handShift new="#jo_t"/>keins von beiden wißt ihr je</line>
                <line xml:id="lo">Gleichmüthig zu beſtehen. Eine widerſpricht <mod rend="strikethrough" hand="#jo_t">der</mod> <mod rend="strikethrough" hand="#jo_t">andern heftig</mod></line>
                <line xml:id="lp" type="inter"><handShift new="#g_bl"/><f:ins f:at="#ins_e" f:orient="right" hand="#g_bl">e</f:ins></line>
                <line xml:id="lq"><handShift new="#jo_t"/>Der andern heftig<handShift new="#g_bl"/>, <handShift new="#jo_t"/>ü<retrace hand="#g_bl">b</retrace>erque<anchor xml:id="ins_e"/>r die andern ihr<handShift new="#g_bl"/>;</line>
                <line xml:id="lr"><handShift new="#jo_t"/>In Freud und Schmerz nur heult <mod rend="strikethrough" hand="#jo_t">ihr</mod> und lacht ihr glei-</line>
                <line xml:id="ls" rend="indent-70">ches Tons<handShift new="#g_bl"/>.</line>
                <line xml:id="lt"><handShift new="#jo_t"/>Nun ſchweigt<handShift new="#g_bl"/>!
                        <handShift new="#jo_t"/>und wartet horchend was die Herr<f:overw>
                        <f:under>i</f:under>
                        <f:over>ſ</f:over>
                    </f:overw>cherin</line>
                <line xml:id="lu">Hochſinnig hier beſchließen mag für ſich und uns.</line>
                <line xml:id="lv" rend="centered"><hi rend="underline">Helena</hi>.</line>
                <line xml:id="lw">Wo biſt du Pythoniſſa<handShift new="#g_bl"/>? <handShift new="#jo_t"/>heiße wie du magſt<handShift new="#g_bl"/>,</line>
                <line xml:id="lx"><handShift new="#jo_t"/>Aus dieſen Gewölben tritt hervor der
                    düſtern Burg<handShift new="#g_bl"/>.</line>
                <line xml:id="ly"><handShift new="#jo_t"/>Gingſt du voraus dem wunderbaren
                    Heldenherren</line>
                <line xml:id="lz">Mich anzukündigen<handShift new="#g_bl"/><retrace hand="#g_t">,</retrace>
                    <handShift new="#jo_t"/><f:overw>
                        <f:under>w</f:under>
                        <f:over>W</f:over>
                    </f:overw>ohl‿<f:overw>
                        <f:under>E</f:under>
                        <f:over>e</f:over>
                    </f:overw>mpfang bereite<f:overw>
                        <f:under>t</f:under>
                        <f:over>nd</f:over>
                    </f:overw> mir<f:overw>
                        <f:under>.</f:under>
                        <f:over><handShift new="#g_bl"/>,</f:over>
                    </f:overw></line>
                <line xml:id="laa"><handShift new="#jo_t"/>So habe Dank und führe ſchnell mich zu
                    ihm ein<handShift new="#g_bl"/>.</line>
                <line f:left="#seg_nur" xml:id="lab" type="inter"><handShift new="#g_t"/>nur.</line>
                <line xml:id="lac"><handShift new="#jo_t"/>Beſchluß der Irrfahrt wünſch
                        ich<handShift new="#g_bl"/>, <handShift new="#jo_t"/>Ruhe
                    wünſ<mod rend="strikethrough">i</mod>ch ich <anchor xml:id="seg_nur"/><mod rend="strikethrough" hand="#g_t">mir</mod><mod rend="strikethrough" hand="#g_t"><handShift new="#g_bl"/>.</mod></line>
                <line xml:id="lad" rend="centered"><handShift new="#jo_t"/><abbr><hi rend="underline">Chorf.</hi></abbr></line>
                <line xml:id="lae">Vergebens blickſt du Königin allſeits herum,</line>
                <line xml:id="laf">Verſchwunden iſt die Häßliche<handShift new="#g_t"/>,
                        <handShift new="#jo_t"/>ſie blieb vielleicht</line>
                <line xml:id="lag">Im Nebel dort<handShift new="#g_t"/>, <handShift new="#jo_t"/>aus deſſen Buſen wir hierher</line>
                <line xml:id="lah">Ich weiß nicht wie gekom̄en, Schnell und ohne
                        Schritt<handShift new="#g_t"/>.</line>
            </zone>
            <surface type="patch">
                <zone f:left="#mainzone" f:top-bottom="#mainzone">
                    <line xml:id="lai"><handShift new="#jo_t"/>Vielleicht auch irrt ſie
                        zweifelhaft im Labyrinth</line>
                    <line xml:id="laj">Der wunderſam zerſtückelt<handShift new="#g_t"/>-<handShift new="#jo_t"/>eins<handShift new="#g_bl"/><retrace hand="#g_t">‿</retrace><handShift new="#jo_t"/>gewordnen
                        Burg</line>
                    <line xml:id="lak">Den Herrn erfragend fürſtlicher Begrüßung halb<handShift new="#g_bl"/>.</line>
                    <line xml:id="lal"><handShift new="#jo_t"/>Doch ſieh dort oben reget ſich in
                        Menge ſchon</line>
                    <line xml:id="lam">In Galerien<handShift new="#g_bl"/>, <handShift new="#jo_t"/>am Fenſter<handShift new="#g_bl"/>, <handShift new="#jo_t"/>in Portalen raſch</line>
                    <line xml:id="lan">Sich hin und her bewegend viele Dienerſchaft<handShift new="#g_bl"/>;</line>
                    <line xml:id="lao"><handShift new="#jo_t"/>Das kündet an vornehm<handShift new="#g_t"/>-<handShift new="#jo_t"/>willkom̄enen Empfang<handShift new="#g_bl"/>.</line>
                </zone>
            </surface>
