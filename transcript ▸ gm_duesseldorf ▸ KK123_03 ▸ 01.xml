        <surface>
            <zone type="main">
                <line rend="centered" xml:id="la">
                    <handShift new="#ri_t_lat"/>
                    <hi rend="underline">Strophe</hi>
                </line>
                <line xml:id="Ach">Ach sie ſtanden noch, Ilions</line>
                <line xml:id="lb">Mauern, aber die Flammengluth</line>
                <line xml:id="lc">Zog, vom Nachbar zum Nachbar schon,</line>
                <line xml:id="ld">Sich verbreitend von hier und dort</line>
                <line xml:id="le">Mit des eigenen Sturmes Hauch</line>
                <line xml:id="lf"><orig>Ue</orig>ber die nächtliche Stadt hin.</line>
                <line rend="centered" xml:id="lg"><hi rend="underline">Antiſtrophe</hi>.</line>
                <line xml:id="lh">Flüchtend sah’ ich durch Rauch und Gluth</line>
                <line type="inter" xml:id="li">
                    <handShift new="#ri_bl_lat"/>
                    <retrace hand="#ri_t_lat">In</retrace>
                    <seg f:left="#zuckenden">
                        <unclear cert="high">
                            <mod rend="strikethrough">Flam</mod>
                        </unclear>
                    </seg>
                </line>
                <line xml:id="lj"><handShift new="#ri_bl_lat"/>Und der <anchor xml:id="zuckenden"/><retrace hand="#ri_bl_lat">zuckenden Flam̄e</retrace><mod rend="strikethrough" hand="#ri_t_lat">n</mod><f:overw>
                        <f:under>l</f:under>
                        <f:over>L</f:over>
                    </f:overw><retrace hand="#ri_bl_lat">oh’n</retrace></line>
                <line f:pos="over" xml:id="lav"><handShift new="#ri_t_lat"/>Und der zuckenden
                    Flamme Loh’n</line>
                <line xml:id="lk"><handShift new="#ri_t_lat"/>Gräſslich zürnender Götter
                    Nah’n</line>
                <line xml:id="ll">Schreitend wundergeſtalte<retrace hand="#ri_bl_lat">n</retrace>.</line>
                <line xml:id="lm">Rieſengroſs durch den düſteren</line>
                <line xml:id="ln">Feuerumleuchteten Qualm hin.</line>
                <line rend="centered" xml:id="lo"><hi rend="underline">Epodos</hi></line>
                <line xml:id="lp">Sah’ ich’s, oder bildete</line>
                <line xml:id="lq">Mir der angſtumſchlungne Geiſt</line>
                <line xml:id="lr">Solches Verworr<mod rend="strikethrough" hand="#ri_bl_lat">e</mod>ne? sagen
                    kann</line>
                <line xml:id="ls">Nimmer ich’s; doch daſs ich dieſs</line>
                <line xml:id="lt">Gräſsliche hier mit Augen schau</line>
                <line xml:id="lu">Solches gewiſs ja weiſs ich.</line>
                <line xml:id="lv">Könnt’ es mit Händen faſſen gar</line>
                <line f:left="#mich" type="inter" xml:id="lw">
                    <handShift new="#ri_bl_lat"/>
                    <retrace hand="#ri_t_lat">von</retrace>
                </line>
                <line xml:id="lx"><handShift new="#ri_t_lat"/>Hielte <anchor xml:id="mich"/><mod rend="strikethrough" hand="#ri_t_lat"><mod rend="strikethrough" hand="#ri_bl_lat">mich von</mod></mod> dem
                    Gefährlichen</line>
                <line f:left="#nur" type="inter" xml:id="ly">
                    <handShift new="#ri_bl_lat"/>
                    <retrace hand="#ri_t_lat">mich</retrace>
                </line>
                <line xml:id="lz"><handShift new="#ri_t_lat"/>Nicht zurücke die Furcht <anchor xml:id="nur"/><mod rend="strikethrough" hand="#ri_t_lat"><mod rend="strikethrough" hand="#ri_bl_lat">nur</mod></mod>.</line>
            </zone>
