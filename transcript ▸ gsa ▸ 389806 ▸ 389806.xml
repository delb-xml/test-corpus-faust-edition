                   <milestone unit="stage" change="#i_r"/>
                   <speaker n="before_8610">Chor der Trojanerinnen</speaker>
                  <lg type="stanza">
                     <l n="8610">Werfet Schweſtern ihr</l>
                     <l n="8611">Traurig gefangenen</l>
                     <l n="8612">Alle Schmerzen weg</l>
                     <l n="8613">Theilet der Herrinn Glück</l>
                     <l n="8614">Theilet Helenens Glück</l>
                     <l n="8615">Welche das Vaterhaus</l>
                     <l n="8616">Mit ſpät wiederkehrendem</l>
                     <l n="8617">Heute mit ſicherem</l>
                     <l n="8618">Fuße betritt</l>
                  </lg>
                  <lg type="stanza">
                     <l n="8619">Preiſet die heiligen</l>
                     <l n="8620">Herſtellenden,</l>
                     <l n="8621">R<orig>u</orig>ckf<orig>u</orig>hrenden Götter.</l>
                     <l n="8622">Denn der Glückliche</l>
                     <l n="8622">Wandelt wie auf mächtigen</l>
                     <l n="8623">Fittigen hingetragen</l>
                     <l n="8624"><orig>U</orig>ber das Rauhſte;</l>
                     <l n="8624 8625">Wenn der Verſtoßene</l>
                     <l><orig>Ue</orig>ber die Ebene</l>
                     <l n="8627">Vergeblich hinckend</l>
                     <l n="todo">Unerreichbar Ziel</l>
                     <l n="todo">In dem Traurigen Auge</l>
                     <l n="8627">Sich müdend verzehrt</l>
                  </lg>
                  <lg type="stanza">
                     <l n="8628">Aber ſie ergriff</l>
                     <l n="8628 8629">Ein Gott die entfernte</l>
                     <l n="8630">Und aus Ilions Schutt</l>
                     <l n="8631">Trug er hierher ſie zurück</l>
                     <l n="8632">In das alte das neu erfriſchte</l>
                     <l n="8633">Vaterhaus</l>
                     <l n="8634">Nach unſäglichen</l>
                     <l n="8635">Freude<unclear cert="high">n</unclear> und Quaalen</l>
                     <l n="8636">Früher Jugend</l>
                     <l n="8637">Eingedenck zu ſeyn.</l>
                  </lg>
