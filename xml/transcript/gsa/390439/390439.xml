                        <l n="5777">
                            <subst f:revType="late">
                                <del>D <space/>
                                </del>
                                <add>Bem<orig>u</orig>h ich mich es wird mir glücken.</add>
                            </subst>
                        </l>
                        <l n="5778">Mich Pantomimiſch <add f:revType="late">deutlich</add> aus zu drücken</l>
                    </sp>
