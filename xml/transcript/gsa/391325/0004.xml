                <!-- 10503ff -->
                <line rend="centered" xml:id="lag">
                    <handShift new="#g_bl"/>
                    <abbr>Feldh</abbr>
                </line>
                <line xml:id="lah">So trete denn der rechte Flügel</line>
                <line rend="indent-70" xml:id="lai">an,</line>
                <line xml:id="laj">Des Feindes rechte, eben jetzt <unclear cert="high">im</unclear></line>
                <line rend="indent-70" xml:id="lak">Steigen</line>
                <line xml:id="lal">Soll eh er noch den letzten Schritt</line>
                <line rend="indent-70" xml:id="lam">gethan</line>
                <line xml:id="lan">Der Jugendkraft, der m<orig>a</orig>chtigen</line>
                <line rend="indent-50" xml:id="lao">Treue weichen</line>
                <line rend="centered" xml:id="lap">Fauſt</line>
                <line xml:id="laq">Erlaube denn daß dieſer muntre</line>
                <line rend="indent-60" xml:id="lar">Held</line>
                <line xml:id="las">Sich ungeſ<orig>a</orig>umt in deine Reihen ſtellt</line>
                <line xml:id="lat">Sich deinen Reihen innigſt einverleibt</line>
                <line xml:id="lau">Und ſo geſellt ſein kräftig weſen</line>
                <line rend="indent-50" xml:id="lav">treibt</line>
                <anchor xml:id="used_1"/>
                <metamark function="used" hand="#g_bl" spanTo="#used_2"/>
                <line xml:id="lbj" rend="centered"><g ref="#g_transp_1"/></line>
