                <line rend="centered" xml:id="law">
                    <choice>
                        <abbr>Ob fh</abbr>
                        <expan>Oberfeldherr</expan>
                    </choice>
                </line>
                <!-- 10519ff -->
                <line xml:id="lax">Der Phalanx unſrer Mitte folge ſacht</line>
                <line xml:id="lay">Dem Feind begegn’ er klug mit aller</line>
                <line rend="indent-60" xml:id="laz">Macht</line>
                <line type="inter" xml:id="lba">
                    <f:ins f:at="#schon" f:orient="right" hand="#g_bl">bereits</f:ins>
                </line>
                <line xml:id="lbb">Ein wenig rechts dort hat <mod rend="strikethrough" hand="#g_bl">ſchon</mod><anchor xml:id="schon"/> er-</line>
                <line rend="indent-50" xml:id="lbc">bittert</line>
                <line xml:id="lbd">Der <app>
                        <lem>unſren</lem>
                        <rdg resp="#wa-app">unſern</rdg>
                    </app> Streitkraft ihren Plan</line>
                <line rend="indent-50" xml:id="lbe">erſch<orig>u</orig>tt<supplied evidence="internal">ert</supplied></line>
                <anchor xml:id="used_2"/>
                <metamark function="used" hand="#g_bl" spanTo="#used_3"/>
                <line rend="indent-40" xml:id="lbf">Fauſt <app><lem>w<gap precision="medium" quantity="3" unit="chars"/> <gap precision="medium" quantity="3" unit="chars"/> <gap precision="medium" quantity="3" unit="chars"/>ch <gap quantity="2" unit="chars"/></lem><rdg resp="#wa-app">winckt rechts</rdg></app></line>
                <line xml:id="lbg">So folge denn auch dieſer d<supplied evidence="internal">einem</supplied> Wort</line>
                <line xml:id="lbh">Er iſt behend, reist alles mit</line>
                <line xml:id="lbi" rend="indent-50">ſich fort.</line>
                <line xml:id="lbk" rend="centered"><g ref="#g_transp_2"/></line>
                <anchor xml:id="used_3"/>
