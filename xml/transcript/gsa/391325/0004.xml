        <surface>
            <zone type="main" xml:id="mainzone">
                <metamark function="used" hand="#g_bl" spanTo="#used_4"/>
                <line f:left="#schwillt" type="inter" xml:id="la"><handShift new="#g_bl"/>ſchleicht</line>
                <line xml:id="lb">Es <anchor xml:id="schwillt"/><mod rend="strikethrough" hand="#g_bl"><unclear cert="high">ſchwillt</unclear></mod>
                    <app>
                        <lem><mod rend="strikethrough" hand="#g_bl"><unclear cert="high">heran</unclear></mod></lem>
                        <rdg resp="#abr-text"><unclear cert="high">heran</unclear></rdg>
                    </app> an aber tauſend Enden</line>
                <line xml:id="lc">Unfruchtbar selbſt, Unfruchtbarkeit zu</line>
                <line rend="indent-60" xml:id="ld">ſpenden</line>
                <line xml:id="le">Es ſchwillt und w<orig>a</orig>chſt <app>
                        <lem>und</lem>
                        <rdg resp="#abr-text"/>
                    </app> komt und <app>
                        <lem><orig>u</orig>ber-</lem>
                        <rdg resp="#abr-text"><orig>u</orig>b</rdg>
                    </app></line><!-- TODO diese und folgende app miteinander verbinden -->
                <line rend="indent-60" xml:id="lf"><app>
                        <lem>zieht</lem>
                        <rdg resp="#abr-text">erzieht</rdg>
                    </app></line>
                <line xml:id="lg">Der wüſten Strecke widerlich Gebiet</line>
                <line xml:id="lh"><mod rend="strikethrough" hand="#g_bl">Da herrſchen Well auf Welle kraft</mod></line>
                <line rend="indent-50" xml:id="li"><mod rend="strikethrough" hand="#g_bl">begeiſtet</mod></line>
                <line xml:id="lj">Zieht ſich zur<orig>u</orig>ck und es iſt nichts
                    geleiſtet</line>
                <space dim="vertical" quantity="1.5" unit="lines"/>
                <line xml:id="lk">Zweckloſe Kraft der wilden Elemente</line>
                <line xml:id="ll">Da <mod rend="erase" hand="#g">wagt mein Geiſt ſich ſelbſt
                        zu</mod> uberfliegen</line>
                <line xml:id="lm">Hier m<orig>o</orig>cht ich <mod rend="erase" hand="#g">k<orig>a</orig>mpfen dies</mod> möcht ich beſiegen<space dim="horizontal" unit="chars" quantity="5"/></line>
                <line xml:id="ln">Und iſt es m<orig>o</orig>glich Woge <gap precision="medium" quantity="9" unit="chars"/></line>
                <line xml:id="lo">An <app>
                        <lem>jede<unclear cert="high">m</unclear></lem>
                        <rdg resp="#abr-text">jeden</rdg>
                    </app><c xml:space="preserve"> </c><gap precision="medium" quantity="15" unit="chars"/></line>
                <line xml:id="lp">
                    <app>
                        <lem/>
                        <rdg resp="#abr-text">
                            <gap quantity="1" unit="words"/>
                        </rdg>
                    </app>
                </line>
                <line xml:id="Geringe">Geringe H<orig>o</orig>he ragt ihr ſtolz
                    entgegen</line>
                <line xml:id="leer"/>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lq">Geringe Tiefe zieht ſie m<orig>a</orig>chtig an</line>
                <line xml:id="lr">Da faſſt ich ſchon im Geiſt den Plan</line>
                <note type="editorial">Lesung Renate Moering</note>
                <line xml:id="ls">
                    <app>
                        <lem>
                            <gap precision="medium" quantity="10" unit="chars"/> göttliche W<gap precision="medium" quantity="5" unit="chars"/>
                        </lem>
                        <rdg resp="#abr-text">
                            <gap quantity="1" unit="chars"/>
                        </rdg>
                    </app>
                </line>
                <line xml:id="lt">Das wilde Meer <gap precision="medium" quantity="3" unit="chars"/> Ufer <gap precision="medium" quantity="3" unit="chars"/></line>
                <line xml:id="lu">Der feuchten Breite Grenzen zu verengen</line>
                <line xml:id="lv">Und weit hinein es in ſich ſelbſt zu drängen</line>
                <line xml:id="lw">Schon Schritt f<orig>u</orig>r Schritt <gap precision="medium" quantity="3" unit="chars"/>
                    <gap precision="medium" quantity="3" unit="chars"/>ch <unclear cert="high">zu
                            er<orig>o</orig>rtern</unclear></line>
                <line xml:id="lx">Das iſt mein Wunſch das wage zu
                    bef<orig>o</orig>rdern</line>
                <space dim="vertical" quantity="1" unit="lines"/>
                <line xml:id="ly"><app>
                        <lem>Das iſt ſehr ehrenhaſt d<gap quantity="2" unit="chars"/> d<gap quantity="2" unit="chars"/>
                            <gap quantity="3" unit="chars"/>ff<unclear cert="high">en</unclear>en</lem>
                        <rdg resp="#abr-text"/>
                    </app>
                </line>
                <space dim="vertical" quantity="1" unit="lines"/>
                <line xml:id="lz">Der Wandrer hinckt an ſeiner Glaubenskrücke</line>
                <line xml:id="laa">Zum Teufelsſtein zur Teufelsbrücke.</line>
                <anchor xml:id="used_4"/>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lab"><handShift new="#g_bl_lat"/><app>
                        <lem>Galilei Dialogus de Systematibus</lem>
                        <rdg resp="#abr-app">Galilei Dialogis d<gap quantity="2" unit="chars"/> Systema</rdg>
                    </app>
                </line>
                <line rend="indent-30" xml:id="lac"><app>
                        <lem>Cosmicis <space dim="horizontal" quantity="3" unit="chars"/><abbr>dial.</abbr> 4</lem>
                        <rdg resp="#abr-app">Cosmicum</rdg>
                    </app>
                </line>
                <line xml:id="lad"><app>
                        <lem>Doppe<supplied>l</supplied>te Benenung der Erde</lem>
                        <rdg resp="#abr-app">Doppe<supplied>l</supplied>te <choice>
                                <unclear cert="high">Benennung</unclear>
                                <unclear cert="low">Bewegung</unclear>
                            </choice>
                            <abbr>vid.</abbr>
                            <abbr>dial.</abbr> 4</rdg>
                    </app>
                    </line>
            </zone>
            <zone f:right-left="#mainzone" xml:id="Ziffern">
                <line f:top="#Geringe" xml:id="lae"><handShift new="#g_bl"/>2<space dim="horizontal" quantity="1" unit="chars"/></line>
                <line f:top="#leer" xml:id="laf">1<space dim="horizontal" quantity="1" unit="chars"/></line>
