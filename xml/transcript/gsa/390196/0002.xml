        <surface>
            <zone rotate="90" type="main" xml:id="mainzone">
                <metamark function="used" hand="#g_bl" spanTo="#used"/>
                <line f:left="#bei" type="inter" xml:id="la"><handShift new="#g_bl"/>nur <app>
                        <lem>ein<unclear cert="low">mal</unclear></lem>
                        <rdg resp="#wa-app" type="substantive"><unclear>einem</unclear></rdg>
                    </app></line>
                <line xml:id="lb">Was Ra<retrace>t</retrace>h hat Rath <anchor xml:id="bei"/><mod rend="strikethrough" hand="#g_bl">bei Menſchen</mod> je gegolten</line>
                <line xml:id="lc">Der beſte Rath erſtarrt im harten Ohr</line>
                <line xml:id="ld">So oft auch That ſich grimmig ſelbſt geſcholten</line>
                <line xml:id="le">So bleibt das Volck ſelbwillig</line>
                <line rend="indent-50" xml:id="lf">wie zuvor</line>
                <space dim="vertical" quantity="1" unit="lines"/>
                <line rend="centered" xml:id="Nereus">Nereus</line>
                <line xml:id="lg">Sinds Menſchenſtimmen die mein Ohr vernimmt</line>
                <line xml:id="lh">Wie es mir gleich im tiefeſten Herzen grimmt</line>
                <line type="inter" xml:id="li"><seg f:left="#strebsam">2</seg><seg f:left="#Gebilde">1</seg></line>
                <line f:right="#selbst" type="inter" xml:id="lj">Stre<anchor xml:id="strebsam"/>bſam Geb<anchor xml:id="Gebilde"/>ilde</line>
                <line xml:id="lk"><mod rend="strikethrough" hand="#g_bl">Das Volck <app>
                            <lem>beſtrebt</lem>
                            <rdg resp="#wa-app" type="substantive">beſtimmt</rdg>
                        </app> ſelbſt</mod><anchor xml:id="selbst"/> G<orig>o</orig>tter zu erreichen</line>
                <line xml:id="ll">Und doch verdammt ſich immer ſelbſt zu</line>
                <line rend="indent-50" xml:id="lm">gleichen</line>
                <line xml:id="ln">Manch Menſchen Alter <anchor xml:id="laesst"/><mod rend="strikethrough" hand="#g_bl">l<orig>a</orig>ßt es mich nicht</mod></line>
                <line f:left="#laesst" type="inter" xml:id="lo">konnt ich g<orig>o</orig>ttlich <f:overw>
                        <f:under><gap unit="chars" quantity="1"/></f:under>
                        <f:over>r</f:over>
                    </f:overw>uhn</line>
                <line rend="indent-50" xml:id="lp">ruhn</line>
                <line xml:id="lq">Doch trieb michs an den Beſten wohlzuthun</line>
                <line xml:id="lr">Doch ſchaut ich <f:overw>
                        <f:under><mod rend="erase" hand="#g"><gap quantity="3" unit="chars" precision="medium"/></mod></f:under>
                        <f:over><unclear cert="low">offen</unclear></f:over>
                    </f:overw>en<space dim="horizontal" quantity="5" unit="chars"/>nach <app>
                        <lem>vollbrachten</lem>
                        <rdg resp="#wa-app">vollbrachter</rdg>
                    </app></line>
                <line rend="indent-60" xml:id="ls"><app>
                    <lem>Thaten</lem>
                    <rdg resp="#wa-app">That</rdg>
                    </app></line><!-- TODO Zusammengehoerigkeit beider app -->
                <line xml:id="lt">So war es ganz als h<orig>a</orig>tt ich</line>
                <line rend="indent-30" xml:id="lu">nich<supplied>t</supplied> gerathen</line>
                <line rend="centered" xml:id="lv"><abbr>Th</abbr></line>
                <line xml:id="lw">Und doch o Greis des Meers vertraut</line>
                <line rend="indent-60" xml:id="lx">man dir</line>
                <line xml:id="ly">Du biſt der Weiſe treib uns nicht von</line>
                <line rend="indent-70" xml:id="lz">hier</line>
                <line xml:id="laa">Schau dieſe Flamme Menſchen <orig>a</orig>hnlich zwar</line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lab">Sie deinem Rath ergiebt ſich ganz und gar</line>
                <anchor xml:id="used"/>
            </zone>
            <zone f:right="#Nereus" f:bottom="#mainzone">
                <line xml:id="lac" rend="indent-30"><handShift new="#sc_t"/><abbr>Sr</abbr> Hochwohlgeboren</line>
                <space dim="vertical" quantity="1" unit="lines"/>
                <line xml:id="lad">des Herrn Geheimerrath von Göthe Excellenz <abbr>p.</abbr></line>
