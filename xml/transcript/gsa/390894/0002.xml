                <line xml:id="lg" type="inter"><f:ins f:at="#der" f:orient="right" hand="#g_bl">allzu läſtigen</f:ins></line>
                <line xml:id="lh">Nun biſt du los <retrace>der</retrace><anchor xml:id="der"/>
                    <space dim="horizontal" quantity="5" unit="chars"/> Schwe<retrace>re</retrace></line>
                <line f:left="#nun" xml:id="li" type="inter">nun friſch</line>
                <line xml:id="lj">Biſt frey <hi rend="underdots"><mod rend="strikethrough">und frank</mod></hi><space dim="horizontal" quantity="3" unit="chars"/><anchor xml:id="nun"/><mod rend="strikethrough">nun</mod><c xml:space="preserve"> </c><app>
                        <lem resp="abr-text"><mod rend="strikethrough">friſch</mod> auf</lem>
