    <text>
        <body>
            <div n="2.1.2.2" type="scene">
                <pb n="2"/>
                <sp>
                    <speaker n="before_5801">Wilder Geſang von Ferne.</speaker>
                    <addSpan f:revType="late" spanTo="#wilde_Heer"/>
                    <lg>
                        <l n="5801">Das wilde Heer</l>
                        <l n="5802">Von <del f:revType="instant">Berge<unclear cert="high">n</unclear></del> berges
                            H<orig>o</orig>hn und duncklem Wald.</l>
                    </lg>
                    <anchor xml:id="wilde_Heer"/>
                    <lg>
                        <l n="5819 5820">Der Faunentanz</l>
                        <l n="5821">In Laub und Kranz</l>
                        <l n="todo">Aus Waldes Nacht</l>
                        <l n="todo">Nur Platz gemacht</l>
                    </lg>
                </sp>
                <sp>
                    <speaker n="before_5807">Plutus.</speaker>
                    <l n="5807">Ich kenne dich du großer Pan</l>
                    <l n="todo">Du kom̄ſt mir ganz erwünſcht heran</l>
                    <l n="5809">Schon weis ich was nicht jeder weis</l>
                    <l n="5810">Und öffne dieſen Zauber Kreis.</l>
                </sp>
                <sp>
                    <speaker n="before_5815">Faunen.</speaker>
                    <lg>
                        <l n="5815">Geputztes Volk</l>
                        <l n="5815">Du Flitterſchau</l>
                        <l n="5816">Sie kom̄en roh</l>
                        <l n="5816">Sie kom̄en rauh</l>
                        <l n="5817">In hohem Sprung</l>
                        <l n="5817">Und raſchem Lauf</l>
                        <l n="5818">Sie treten derb</l>
                        <l n="5818"><sic>Unch</sic> tüchtig auf</l>
                        <l n="5805">Wir wiſſen doch was keiner weis</l>
                        <l n="5806">Und dringen in den leeren Kreis</l>
                    </lg>
                </sp>
                <delSpan f:revType="soon" spanTo="#Plutus"/>
                <!-- Ersetzungsverhältnis? -->
                <sp>
                    <speaker n="before_5807">Plutus.</speaker>
                    <l n="5807">Ich kenne dich vermumter Pan</l>
                    <l n="5808">Du haſt einen kühnen Schritt gethan</l>
                    <l n="todo">
                        <del f:revType="soon" change="#change_l5812">Sie dringen ein wie wirds
                            ergehn</del>
                    </l>
                    <l n="5812">
                        <add f:revType="soon" change="#change_l5812">Doch wunderlichſtes kann
                            geſchehn</add>
                    </l>
                    <l n="5814">Sie haben ſich nicht vorgeſehn.</l>
                </sp>
                <anchor xml:id="Plutus"/>
                <pb n="3"/>
                <sp>
                    <l n="5864">Die wilden Männer ſinds genannt</l>
                    <l n="5865">Auf Harz und Gulden wohl bekannt</l>
                    <l n="5866">Natürlich hier in aller Kraft</l>
                    <l n="5867">Sie kom̄en ſämtlich rieſenhaft</l>
                    <l n="5868"><choice>
                            <sic>Denn</sic>
                            <corr>Den</corr>
                        </choice> Fichtenſtamm in rechter Hand</l>
                    <l n="5869">Und um den Leib ein wulſtig Band</l>
                    <l n="5870"><del f:revType="instant">Ein</del> Den derbſten Schurz von Zweig und
                        Blatt</l>
                    <l n="5871">Leibwache wie ſie niemand hat</l>
                </sp>
            </div>
