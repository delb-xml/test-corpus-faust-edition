        <surface>
            <zone rotate="270" type="main" xml:id="mainzone">
                <line xml:id="lab"><handShift new="#g_tr"/><mod rend="strikethrough" hand="#g_tr">Du ſey Erztruchſeß.
                        A</mod></line>
                <line xml:id="Dich">Dich wähl ich zum Erztruchſeß! Alſo ſey fortan,</line>
                <line xml:id="laa">Dir Jagd, Geflügel-Hof und Vorwerck unterthan;</line>
                <line xml:id="lz"><f:overw><f:under>Die</f:under><f:over>Der</f:over></f:overw> Lieblingsſpeiſe<mod rend="strikethrough" hand="#g_tr"><hi rend="underdots" hand="#g_tr">n</hi></mod> <mod rend="strikethrough" hand="#g_tr">la</mod> Wahl, laß mir zu allen Zeiten</line>
                <line f:left="#mir" xml:id="ly" type="inter">und</line>
                <line xml:id="lx">Wie ſie der Monat bringt, <anchor xml:id="mir"/><mod rend="strikethrough" hand="#g_tr">mir</mod> ſorgſam zubereiten.</line>
                <line xml:id="lw" rend="centered"><abbr>Erztr.</abbr></line>
                <line xml:id="lv">Streng Faſten ſey f<orig>u</orig>r mich die angenehmſte
                    Pflicht,</line>
                <line xml:id="lu">Bis, vor dich hingeſtellt, dich freut ein
                    Wohlgericht.</line>
                <line xml:id="lt">Der Küche Dienerſchaft ſoll ſich mit mir vereinigen,</line>
                <line xml:id="ls">Das Ferne beyzuziehn, die Jahrszeit zu beſchleunigen.</line>
                <line xml:id="lr" type="inter" rend="centered"><g ref="#g_transp_6"/></line>
                <line xml:id="lq" rend="indent-center-60">Kayſer (zum Vierten)</line>
                <line xml:id="Weil">Weil unausweichlich hier ſich nur vom Faſten
                    handelt,</line>
                <line xml:id="lp">So ſey mir, junger Held, zum Schencken umgewandelt.</line>
                <note type="editorial">Kommata später</note>
                <line xml:id="lo">Erzſchencke ſorge nun daß unſre Kellerey</line>
                <line xml:id="ln">Auf’s reichlichſte <mod rend="strikethrough" hand="#g_tr">be</mod> verſorgt mit
                    guten Weinen ſey.</line>
                <line xml:id="lm" type="inter"><f:ins f:at="#selbst" f:orient="right" hand="#g_tr">ſey</f:ins></line>
                <line xml:id="ll">Du<mod rend="strikethrough" hand="#g_tr">,</mod> ſelb<f:overw>
                        <f:under>er</f:under>
                        <f:over>ſt</f:over>
                    </f:overw><anchor xml:id="selbst"/> mäßig, laß nicht über
                    Heiterkeiten,</line>
                <line xml:id="lk">Durch der Gelegenheit Verlocken, dich verleiten.</line>
                <line xml:id="lj" rend="centered"><abbr>Erz S</abbr></line>
                <line xml:id="li">Mein Fürſt die Jugend ſelbſt, wenn man ihr nur
                    vertraut,</line>
                <line xml:id="lh">Steht, eh man ſich’s verſieht zu Männern auferbaut.</line>
                <space dim="vertical" quantity="2" unit="lines"/>
            </zone>
            <zone f:bottom-top="#mainzone" f:right-left="#mainzone" rotate="270">
                <line xml:id="lg"><handShift new="#xx_bl_lat"/><fw type="sig"><num>II</num>/<hi rend="sub">5</hi> H<hi rend="sup">20</hi></fw><space dim="horizontal" quantity="1" unit="chars"/></line>
            </zone>
            <zone f:bottom-top="#mainzone" f:left-right="#mainzone" rotate="270">
                <line xml:id="lf"><handShift new="#g_tr"/><fw type="folioNum">2.</fw></line>
            </zone>
            <zone f:left-right="#mainzone" f:bottom="#mainzone">
                <line xml:id="le"><handShift new="#jo_t"/>Reden und Äußerungen hin</line>
                <line xml:id="ld" rend="indent-20">und wieder.</line>
            </zone>
            <zone f:right-left="#mainzone" f:top="#Dich">
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="la"><handShift new="#g_tr"/><g ref="#g_transp_6"/>
                    <anchor xml:id="Dich1"/>Dich reitzt nicht fern und früh womit die Tafel
                    prangt,</line>
                <line xml:id="lb" f:left="#Dich1">Einfach und kräftig iſt’s wornach dein
                    Sinn</line>
                <line xml:id="lc" rend="indent-70">verlangt.</line>
            </zone>
