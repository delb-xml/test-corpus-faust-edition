    <sourceDoc>
        <surface>
            <zone type="main" xml:id="Hauptzone">
                <metamark function="used" hand="#g_bl" spanTo="#Schluss"/>
                <line rend="indent-20" xml:id="la"><handShift new="#we_t"/>Herrn Varnhagen von Enſe.</line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line rend="indent-10" xml:id="lb"><abbr>Ew Hochwohlgeb<g ref="#truncation"/><anchor xml:id="Hochwohlgeb"/></abbr></line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line f:left="#Hochwohlgeb" xml:id="lc">machen mir durch meine frühern Briefe an</line>
                <line xml:id="ld">den trefflichen Wolf, von Ihrer eignen Hand geſchrieben, ein gar</line>
                <line type="inter" xml:id="le"><handShift new="#g_t"/><f:ins f:at="#Sie" f:orient="right" hand="#g_t">Dieſe Blätter</f:ins></line>
                <line xml:id="lf"><handShift new="#we_t"/>ſchönes Geſchenk. <mod rend="strikethrough" hand="#g_t">Sie</mod><anchor xml:id="Sie"/> geben, wenn man ſie auch nicht von be-</line>
                <line type="inter" xml:id="lg"><handShift new="#g_t"/><f:ins f:at="#sind" f:orient="right" hand="#g_t">findet geben</f:ins></line>
                <line xml:id="lh"><handShift new="#we_t"/>ſonderer Bedeutung <mod rend="strikethrough" hand="#g_t">ſind</mod><anchor xml:id="sind"/>, doch Zeugniß von einem freyem heitern</line>
                <line xml:id="li">Daſeyn und einem reinen wechſelſeitigen Vertrauen. Nehmen</line>
                <line xml:id="lj">Sie dafür meinen ſchönſten Dank und zeichnen mich unter Ihre Schuldner.</line>
                <line rend="indent-10" xml:id="lk"><orig>Ue</orig>ber die Berliner Jahrbücher hätte ich wohl gern mit Ihnen ein</line>
                <line xml:id="ll">traulich Wort geſprochen. Ganz ohne Frage iſt es ein großes Ver-</line>
                <line xml:id="lm">dienſt dieſer Blätter, daß die Rezenſenten ſich namentlich unter-</line>
                <line type="inter" xml:id="ln">
                    <handShift new="#g_t"/>
                    <seg f:left="#ist">2</seg>
                    <seg f:left="#dieses">3</seg>
                    <seg f:left="#mir">1</seg>
                </line>
                <line xml:id="lo"><handShift new="#we_t"/>zeichnen<f:overw><f:under>,</f:under><f:over><handShift new="#g_t"/>.</f:over></f:overw><c xml:space="preserve"> </c><handShift new="#we_t"/>
                    <f:overw>
                        <f:under>b</f:under>
                        <f:over><handShift new="#g_t"/>B</f:over>
                    </f:overw><handShift new="#we_t"/>eſonders <anchor xml:id="ist"/>iſt die<anchor xml:id="dieses"/>ſes m<anchor xml:id="mir"/>ir <mod rend="strikethrough" hand="#g_t">gar</mod> ſehr viel werth, da ich der</line>
                <line type="inter" xml:id="lp"><handShift new="#g_t"/><f:ins f:orient="right" f:at="#lq" hand="#g_t">fortſchreitenden in</f:ins><f:ins f:at="#nicht" f:orient="right" hand="#g_t">durchaus</f:ins></line>
                <line xml:id="lq"><handShift new="#we_t"/>Literatur <mod rend="strikethrough" hand="#g_t">und</mod> ihren Zweigen nicht<anchor xml:id="nicht"/> folgen kan̄; ſo werden mir</line>
                <line type="inter" xml:id="lr"><handShift new="#g_t"/><f:ins f:at="#dadurch" f:orient="right" hand="#g_t">durch ſolche Vermittlung</f:ins></line>
                <line xml:id="ls"><handShift new="#we_t"/><mod rend="strikethrough" hand="#g_t">dadurch</mod><anchor xml:id="dadurch"/> die bedeutenden Männer bekannt, welche ſich jetzt in</line>
                <line type="inter" xml:id="lt" f:right="#eigne"><handShift new="#g_t"/><mod rend="strikethrough" hand="#g_t">und ſich beredten machen</mod> und</line>
                <line xml:id="lu"><handShift new="#we_t"/>den verſchieden<handShift new="#g_t"/>ſt<handShift new="#we_t"/>en Fächern hervorthun<retrace hand="#g_t">,</retrace> <mod rend="strikethrough" hand="#g_t">theils</mod> durch <mod rend="strikethrough" hand="#g_t">die</mod> eigne<anchor xml:id="eigne"/></line>
                <line type="inter" xml:id="lv"><handShift new="#g_t"/><seg f:left="#Arbeit">ſowoh<supplied evidence="internal">l</supplied></seg><seg f:right="#durch">als auch</seg></line>
                <line xml:id="Arbeit"><handShift new="#we_t"/>Arbeit, <mod rend="strikethrough" hand="#g_t">theils</mod> durch<anchor xml:id="durch"/> die Aufnahme in Ihren Kreis Aufmerksamkeit</line>
                <line xml:id="lw">erregen und Würdigung gewinnen.</line>
                <line rend="indent-10" xml:id="lx">Eine hiebey unvermeidliche Gefahr iſt nicht leicht abzulehnen,</line>
                <line xml:id="ly">daß nämlich einer und der andere irgend etwas Falſches, zwar</line>
                <line xml:id="lz">unter ſeinem Namen aber doch in ſo guter und ſtaatlicher Ge-</line>
                <line xml:id="laa">ſellſchaft vortrage daß auch ein <f:overw><f:under>v</f:under><f:over>V</f:over></f:overw>erfängliches und Schädliches</line>
                <line xml:id="lab">Zutrauen und Beyſtimmung gewinnen könne. Wenn <abbr>z E.<g ref="#truncation"/></abbr></line>
                <line xml:id="lac">Bourgignie ganz unbewunden ausſpricht, daß man die wahre</line>
                <line xml:id="lad">dem Menſchen ſo nöthige Heautognoſie bey Hypochondriſten</line>
                <line f:left="#koenne" xml:id="lae" type="inter"><handShift new="#g_t"/>ſolle</line>
                <line xml:id="laf"><handShift new="#we_t"/>und Humoriſten lernen <anchor xml:id="koenne"/><mod rend="strikethrough" hand="#g_t">könne</mod>, ſo iſt dieſes eine ſo gefahrvolle</line>
                <line xml:id="lag"><orig>Ae</orig>ußerung als nur irgend eine; denn nichts ist bedenklicher als</line>
                <line xml:id="lah">die Schwäche zur Maxime zu machen. Leidet doch die bildende</line>
                <line xml:id="lai">Kunſt der Deutſchen ſeit dreyßig Jahren an der Verfolgung</line>
                <line xml:id="laj">dieſer Schwäche. Vor ſolchen ſchmeichelhaften Irrthümern</line>
                <line xml:id="lak">fürchte ich mich; denn wer ſoll ſie entdecken?<space dim="horizontal" quantity="4" unit="chars"/>Verzeihen</line>
                <line xml:id="lal">Sie dieſe Betrachtungen, ich bin ſie einem ſchönen offenen</line>
                <line xml:id="lam">Verhältniß zu Ihnen ſchuldig.</line>
                <line rend="centered" type="inter" xml:id="Einweisung">
                    <handShift new="#g_t"/>
                    <g ref="#g_transp_1"/>
                </line>
                <line xml:id="lan" rend="indent-10"><handShift new="#we_t"/>Wiederholten Dank und die beſten Grüße der theuren Ge-</line>
                <line xml:id="lao">fährtin Ihres Lebens.</line>
                <line xml:id="lap">Weimar den <date>8. <abbr>Nov.</abbr> 1827.</date></line>
                <anchor xml:id="Schluss"/>
            </zone>
            <zone f:bottom="#Arbeit" f:right-left="#Hauptzone" rotate="90" xml:id="Einweisungszeichen">
                <space dim="vertical" quantity="1" unit="lines"/>
                <line xml:id="laq"><handShift new="#g_t"/>Heautontimorumenen<space dim="horizontal" quantity="4" unit="chars"/><choice><orig>Purkinie</orig><reg>Purkinje</reg></choice>,</line>
            </zone>
            <zone f:left-right="#Hauptzone" f:top-bottom="#lav" rotate="270">
                <line xml:id="lar">
                    <handShift new="#g_t"/>
                    <g ref="#g_transp_1"/><space dim="horizontal" quantity="1" unit="chars"/>
                </line>
            </zone>
            <zone f:left-right="#Hauptzone" f:bottom="#lal" rotate="270">
                <metamark function="used" hand="#g_bl" spanTo="#koennen2"/>
                <line xml:id="lav" type="inter"><handShift new="#jo_t"/><f:ins f:at="#die" f:orient="left" hand="#jo_t">Ih<f:overw><f:under>r</f:under><f:over>n</f:over></f:overw>en</f:ins></line>
                <line xml:id="las">Wie glücklich aber habe ich Sie zu preiſen daß <mod rend="strikethrough">die</mod><anchor xml:id="die"/> auf die Stimme Hegels und <choice><orig>Humbolds</orig><reg>Humboldts</reg></choice></line>
                <line xml:id="lat">dieſen Winter zu horchen vergönnt iſt; die Weimariſchen Freunde die in aller Stille ſo</line>
                <line xml:id="lau">gern ſchönes und Gutes aufnehmen werden ſich hoffentlich auf irgend einem Wege</line>
                <line xml:id="law">auch ihren Theil beſcheiden zueignen können.</line>
                <anchor xml:id="koennen2"/>
            </zone>
        </surface>
    </sourceDoc>
