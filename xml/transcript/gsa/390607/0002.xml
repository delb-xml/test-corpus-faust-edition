                <line xml:id="lat" type="inter"><handShift new="#g_t"/><seg f:left="#lav">Obſchon</seg><seg f:left="#thoerig">hirnlos</seg></line>
                <line xml:id="lau" type="inter"><handShift new="#g_bl"/>Wenn dich verk<f:overw>
