        <surface>
            <zone type="main" xml:id="Hauptzone" rotate="180">
                <line xml:id="la"><handShift new="#g_bl"/>Genug ihr ſeyd verlohren alſo friſch ans W<supplied evidence="internal">erk</supplied></line>
                <line xml:id="lb" rend="indent-center-60"><f:grLine f:orient="horiz" hand="#g_bl" f:style="linear"/> (klatſcht</line>
