                <line xml:id="lao" type="inter"><seg f:left="#scheinst">2</seg>
                    <seg f:left="#weise">1</seg></line>
                <line xml:id="lap">Du biſt erfahren, ſche<anchor xml:id="scheinst"/>ineſt we<anchor xml:id="weise"/>iſe<handShift new="#g_t"/>,</line>
                <line xml:id="laq" rend="indent-50"><handShift new="#g_bl"/>gut geſinnt</line>
                <line f:left="#Wenn" xml:id="lar" type="inter">dich</line>
                <line xml:id="las">Wenn<anchor xml:id="Wenn"/><c xml:space="preserve"> </c><mod rend="strikethrough" hand="#g_bl">dieſe <app>
