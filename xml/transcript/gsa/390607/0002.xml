                <line xml:id="lv" rend="indent-40"><handShift new="#g_bl"/><mod rend="strikethrough" hand="#g_bl">getrennt</mod> getrennten Haupts</line>
                <line f:pos="over" xml:id="lw" rend="indent-60"><handShift new="#g_t"/>Haup<mod rend="strikethrough" hand="#g_t">ſ</mod>ts,</line>
