                <line f:left="#eingewickelt" xml:id="lr" type="inter"><mod rend="strikethrough" hand="#g_bl">eingeh<orig>u</orig>llt</mod> endlich</line>
                <line xml:id="ls">Und <anchor xml:id="eingewickelt"/><mod rend="strikethrough" hand="#g_bl">eingewickelt</mod> <mod rend="strikethrough" hand="#g_bl">zuletzt</mod> <mod rend="strikethrough" hand="#g_bl">war ſch<orig>o</orig>nes Haupt</mod></line>
                <line f:pos="over" xml:id="lt"><handShift new="#g_t"/>Und eingewickelt endlich, zwar<anchor xml:id="zwar"/> getrennten</line>
