            <zone f:bottom="#Dass" f:right-left="#mainzone" rotate="270">
                <line xml:id="lab"><handShift new="#g_bl"/>Die Dioskuren hatten jener Zeit</line>
                <line xml:id="lac">Das Schweſterchen aus <app>
                        <lem>R<orig>a</orig>uberhand</lem>
                        <rdg resp="#wa-app">Räuber Hand</rdg>
                    </app> befreyt</line>
                <line xml:id="lad">Doch dieſe nicht gewohnt beſiegt zu ſ<unclear cert="high">ey</unclear>n</line>
                <line xml:id="lae">Ermannten ſich und eilten hinterdrein</line>
