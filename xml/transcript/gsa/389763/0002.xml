        <surface>
            <zone type="main" xml:id="mainzone">
                <metamark function="used" hand="#g_ro" spanTo="#used"/>
                <line xml:id="la" rend="centered"><handShift new="#g_bl"/>Fauſt</line>
                <line xml:id="lb">Von Herkules willſt nichts <unclear cert="high">erwä</unclear>hnen.</line>
                <line xml:id="lc" rend="centered">Chiron</line>
                <line xml:id="ld">O! Weh! errege nicht mein Sehnen</line>
                <line xml:id="le">Ich hatte Phöbus nie geſehn,</line>
                <line xml:id="lf">Noch Mars noch Hermes, wie ſie heiſ<supplied evidence="internal">ſen</supplied></line>
                <line xml:id="lg">Doch ſah ich mir vor Augen ſtehn</line>
                <line xml:id="lh">Was alle Me<retrace>n</retrace>ſchen göttlich preiſen</line>
                <line xml:id="li" rend="indent-10">So war er ein geborner K<orig>o</orig>nig</line>
                <line xml:id="lj">Als Jüngling herrlichſt anzuſchaun</line>
                <line xml:id="lk">Dem ältern Bruder unterthänig</line>
                <line xml:id="ll" type="inter">Und auch</line>
                <line xml:id="lm"><mod rend="strikethrough" hand="#g_bl">So wie</mod> den allerliebſten Fraun.</line>
                <line xml:id="ln"><mod rend="strikethrough" hand="#g_bl">Die Erde bringt <anchor xml:id="ihn"/><mod rend="strikethrough" hand="#g_bl">ihn ſo nicht</mod> wieder</mod></line>
                <line f:left="#ihn" xml:id="lo" type="inter">das <mod rend="strikethrough" hand="#g_bl">Gleiche</mod></line>
                <line xml:id="lp" rend="indent-30"><mod rend="strikethrough" hand="#g_bl">Olymp</mod>
                    <seg f:left="#ein">ein</seg></line>
                <line xml:id="lq">Vergebens <anchor xml:id="muehen"/>mühen ſich die Lieder</line>
                <line xml:id="lr">Vergebens quälen ſie den St<anchor xml:id="ein"/>ein</line>
                <line xml:id="ls">Ha<seg rend="inbetween">l</seg>t</line>
                <space dim="vertical" unit="lines" quantity="0.5"/>
                <mod rend="strikethrough" spanTo="#Hebe" hand="#g_bl"/>
                <line xml:id="lt">Nicht Gäa ze<unclear cert="high">ugt</unclear> ſich ſolche wieder</line>
                <line xml:id="lu">Zum Himmel <app>
                        <lem>Hebe</lem>
                        <rdg resp="#wa-app #rfl" type="substantive">hebt</rdg>
                    </app></line>
                <anchor xml:id="Hebe"/>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lv">Den zweyten zeugt nicht Gäa wieder</line>
                <line xml:id="lw">Nicht f<orig>u</orig>hrt ihn Hebe Himmelein,</line>
                <space dim="vertical" quantity="1" unit="lines"/>
                <line xml:id="lx" rend="indent-40">In der <app>
                        <lem>gr<orig>o</orig>ßten</lem>
                        <rdg resp="#rfl">großen</rdg>
                    </app> Eile</line>
                <line xml:id="Dass">Daß <app>
                    <lem><mod rend="strikethrough" hand="#g_bl">d<gap unit="chars" quantity="2" precision="medium"/></mod></lem>
                    <rdg resp="#rfl">im<supplied evidence="internal" cert="low">mer</supplied></rdg>
                </app> ich an dieſem Platz verweile</line>
                <space dim="vertical" quantity="1" unit="lines"/>
                <line xml:id="ly" rend="indent-60">den <anchor xml:id="Unglueck"/>U<gap unit="chars" quantity="7" precision="medium"/></line>
                <line f:left="#Unglueck" xml:id="lz"><gap unit="chars" quantity="4" precision="medium"/></line>
                <anchor xml:id="used"/>
