        <surface>
            <zone type="main" xml:id="Hauptzone">
                <space dim="vertical" quantity="1" unit="lines"/>
                <metamark function="used" spanTo="#Hupft" hand="#g_bl"/>
                <line xml:id="lm"><handShift new="#g_bl_lat"/>Doch auf einmal ein
                        Gel<orig>a</orig>chter</line>
                <line xml:id="ln">Echo’t in <app>
                        <lem><f:overw>
                                <f:under>dem</f:under>
                                <f:over>n</f:over>
                            </f:overw></lem>
                        <rdg resp="abr-text">den</rdg>
                    </app> Holen Raume</line>
                <line xml:id="lo">Sehe mich um da springt ein Knabe</line>
                <line xml:id="lp">Von der Mutter schoos zum Vater</line>
                <line xml:id="lq">Nackt wie amor, <mod rend="strikethrough">glatt und ko</mod>ſe<supplied evidence="internal">nd</supplied></line>
                <line xml:id="lr"><space dim="horizontal" quantity="17" unit="chars"/>nett und
                    zierlich</line>
                <line xml:id="ls">Etwas bl<orig>a</orig>ſslich doch behende</line>
                <line xml:id="lt">Springt er bald zum Boden nieder</line>
                <line xml:id="lu">H<orig>u</orig>pft von</line>
                <anchor xml:id="Hupft"/>
                <metamark function="used" spanTo="#bewegen" hand="#g_bl"/>
                <line xml:id="lv"><handShift new="#g_t_lat"/><space dim="horizontal" quantity="27" unit="chars"/>schwer zu sagen</line>
                <line xml:id="lw">Ist es Goldschmuck? ist es <app>
