        <surface>
            <zone type="main" xml:id="mainzone">
                <line f:pos="between" rend="centered" xml:id="la"><handShift new="#g_bl"/>Erzkanzler</line>
                <line rend="centered" xml:id="lb"><handShift new="#g_t"/><hi rend="underline" hand="#g_t">Erzkanzler</hi></line>
                <line type="inter" xml:id="lc"><handShift new="#g_t"/><f:ins f:at="#Demuth" f:orient="right" hand="#g_t">tiefſter Bruſt in</f:ins></line>
                <line f:pos="between" xml:id="ld"><handShift new="#g_bl"/><unclear cert="high">Deine</unclear> <gap quantity="4" unit="chars" precision="medium"/> <unclear cert="high">ſtehen</unclear><gap precision="medium" quantity="12" unit="chars"/>wir <abbr>Geb</abbr></line>
                <line f:pos="over" xml:id="le"><handShift new="#g_t"/>Mit Stolz i<f:overw>
                        <f:under><mod rend="strikethrough" hand="#g_t">m</mod></f:under>
                        <f:over>n</f:over>
                    </f:overw><c xml:space="preserve"> </c><mod rend="strikethrough" hand="#g_t">Herzen</mod>, <anchor xml:id="Demuth"/>Demuth an Gebärde</line>
                <line type="inter" xml:id="Fuersten"><handShift new="#g_t"/>Fürſten dir gebeug<supplied>t</supplied>, die erſten auf der Erde</line>
                <line f:pos="over" xml:id="lf"><handShift new="#g_t"/><mod rend="strikethrough" hand="#g_t">Vor dir <anchor xml:id="gebeugt"/>gebeugt</mod></line>
                <line f:left="#gebeugt" type="inter" xml:id="lg"><handShift new="#g_t"/><mod rend="strikethrough" hand="#g_t"><gap precision="medium" quantity="3" unit="chars"/></mod></line>
                <line f:pos="over" xml:id="lh"><handShift new="#g_t"/>Solang das treue <mod rend="strikethrough" hand="#g_t">Bruſt</mod> Blut die vollen Adern</line>
                <line rend="indent-70" xml:id="li"><handShift new="#g_t"/>regt</line>
                <line f:pos="between" xml:id="lj"><handShift new="#g_bl"/>Vor Euch dir gebeugt <gap precision="medium" quantity="8" unit="chars"/> Erde</line>
                <line f:pos="between" xml:id="lk"><handShift new="#g_bl"/>Solang das Herz in ſeinen Adern ſchl<orig>a</orig>gt</line>
                <line f:pos="over" xml:id="ll"><handShift new="#g_t"/>Sind wir der Körper den dein Wille leicht bewegt.</line>
                <line f:pos="between" xml:id="lm"><handShift new="#g_bl"/>Wir ſind der K<unclear cert="high"><orig>o</orig>rper</unclear> den dein Wille <gap precision="medium" quantity="3" unit="chars"/> bewegt</line>
                <line f:pos="over" rend="centered" xml:id="ln"><handShift new="#g_t"/><hi rend="underline" hand="#g_t"><abbr>K.</abbr></hi></line>
                <line xml:id="lo">Und alſo ſey, zum Schluß, was wir bisher bethätigt,</line>
                <line xml:id="lp">Für alle Folgezeit durch Schrift und Zug beſtätigt.</line>
                <line f:pos="between" xml:id="lq"><handShift new="#g_bl"/>Und alſo ſey jenes was wir <unclear cert="high">bisher</unclear></line>
                <line f:pos="over" xml:id="lr"><handShift new="#g_t"/>Zwar habt ihr den Beſitz als Herren v<orig>o</orig>llig frey</line>
                <line f:pos="between" rend="indent-70" xml:id="ls"><handShift new="#g_bl"/>beth<orig>a</orig>tigt</line>
                <line f:pos="over" xml:id="lt"><handShift new="#g_t"/>Mit dem Beding jedoch daß er untheilbar ſey.</line>
                <line f:pos="between" xml:id="lu"><handShift new="#g_bl"/>F<orig>u</orig>r alle <unclear cert="low">folgenden</unclear>Folgezeit Schrifften uns beſtätigt.</line>
                <line f:pos="over" xml:id="lv"><handShift new="#g_t"/>Und wie ihr auch vermehrt was ihr von uns</line>
                <line rend="indent-60" xml:id="lw">empfangen</line>
                <line f:pos="between" xml:id="lx"><handShift new="#g_bl"/><gap precision="medium" quantity="3" unit="chars"/> <unclear cert="low">tätigende</unclear>vom Beſitz<space dim="horizontal" quantity="6" unit="chars"/>untheilbar ſey</line>
                <note type="editorial">V. 10967 und 10968 hier offensichtlich noch in einem Vers und mit Spatium</note>
                <line f:pos="over" xml:id="ly"><handShift new="#g_t"/>Es ſolls der <orig>a</orig>ltſte Sohn in gleichem Maas</line>
                <line rend="indent-60" xml:id="lz">erlangen.</line>
                <line f:pos="between" xml:id="laa"><handShift new="#g_bl"/>Auch ſei<space dim="horizontal" quantity="10" unit="chars"/>wie ihr s empfangen</line>
                <line f:pos="over" rend="centered" xml:id="lab"><handShift new="#g_t"/><hi rend="underline" hand="#g_t"><abbr>EK.</abbr></hi></line>
                <line f:pos="between" xml:id="lac"><handShift new="#g_bl"/>Es ſolls der <unclear cert="high"><orig>A</orig></unclear>ltſte ſich die Felder Land erlangen</line>
                <line f:pos="over" type="inter" xml:id="lad"><handShift new="#g_t"/>alsbald</line>
                <line xml:id="lae">Dem Pergament <anchor xml:id="sogleich"/><mod rend="strikethrough" hand="#g_t">ſogleich</mod> vertrau ich wohlgemuth</line>
                <line type="inter" xml:id="laf"><seg f:left="#Dem">3</seg><seg f:left="#Reich">4</seg><seg f:left="#und">5</seg><seg f:left="#uns"><f:overw>
                            <f:under>5</f:under>
                            <f:over>6</f:over>
                        </f:overw></seg><seg f:left="#zum">1</seg><seg f:left="#Glueck">2</seg></line>
                <line xml:id="lag">D<anchor xml:id="Dem"/>em Re<anchor xml:id="Reich"/>ich <anchor xml:id="und"/>und <anchor xml:id="uns"/>uns <f:overw>
                        <f:under>z</f:under>
                        <f:over>Z</f:over>
                    </f:overw><anchor xml:id="zum"/>um G<anchor xml:id="Glueck"/>lück, das wichtigſte Statut,</line>
                <line f:pos="between" xml:id="lah"><handShift new="#g_bl"/>Dem Pergament alsbald vertrau ich wohlgemuth</line>
                <line f:pos="over" xml:id="lai"><handShift new="#g_t"/>Reinſchrift und Sieglung ſoll gleich die Canzley be-</line>
                <line f:pos="between" xml:id="laj"><handShift new="#g_bl"/>Dem Reich wie <unclear cert="high">uns zum Glück das wichtigſte</unclear> Statut</line>
                <line f:pos="over" rend="indent-60" xml:id="lak"><handShift new="#g_t"/>ſchäftigen,</line>
                <line xml:id="lal">Mit heiliger Signatur wirſt du’s der Herr</line>
                <line f:pos="between" xml:id="lam"><handShift new="#g_bl"/><gap precision="medium" quantity="3" unit="chars"/> <gap precision="medium" quantity="5" unit="chars"/>Schrift und ſiegelung <unclear cert="high">ſoll</unclear><gap precision="medium" quantity="4" unit="chars"/><unclear cert="low">beſchäftigen</unclear></line>
                <line f:pos="over" rend="indent-60" xml:id="lan"><handShift new="#g_t"/>bekräftigen.</line>
                <line f:pos="between" xml:id="lao"><handShift new="#g_bl"/><mod rend="strikethrough" hand="#g_bl">Dann</mod> Mit der Signatur d<unclear cert="high">as</unclear> heilig zu bekräftigen</line>
                <line f:pos="over" rend="centered" xml:id="lap"><handShift new="#g_t"/><hi rend="underline" hand="#g_t">Kayſer</hi></line>
                <line xml:id="laq">Und ſo entlaß ich euch, damit den großen Tag</line>
                <line f:pos="between" xml:id="lar"><handShift new="#g_bl"/>Und ſo entlaß ich euch damit von dieſem Haus</line>
                <line f:pos="over" xml:id="las"><handShift new="#g_t"/>Geſammelt jedermann ſich überlegen mag.</line>
                <line type="inter" xml:id="lat"><f:ins f:at="#Fuersten1" f:orient="right" hand="#g_t">weltlichen</f:ins></line>
                <line f:pos="between" rend="indent-60" xml:id="lau"><handShift new="#g_bl"/><unclear cert="high">Tag</unclear></line>
                <line f:pos="over" rend="indent-40" xml:id="lav"><handShift new="#g_t"/><hi rend="underline" hand="#g_t">Die</hi> <anchor xml:id="Fuersten1"/><hi rend="underline" hand="#g_t">F<orig>u</orig>rſten</hi> (entfern<f:overw>
                        <f:under>ſ</f:under>
                        <f:over>e</f:over>
                    </f:overw>n ſich)</line><!-- FIXME 2x hi und dazwischen anchor aendern in ein gesamtes <hi> -->
                <line f:pos="between" xml:id="law"><handShift new="#g_bl"/>Die Fürſten gehen fort</line>
                <line f:pos="over" rend="indent-40" xml:id="lax"><handShift new="#g_t"/>Der Geiſtliche (bleibt und ſpricht)</line>
                <line xml:id="lay" rend="indent-40"><mod rend="strikethrough" hand="#g_t">Der</mod><c xml:space="preserve"> </c>Erzkanzler <mod rend="strikethrough" hand="#g_t"><choice>
                            <sic>fogt</sic>
                            <corr>folgt</corr>
                        </choice> (bl</mod>eibt</line>
                <line xml:id="laz">Der Canzler ging hinweg der Biſchof iſt geblieben</line>
                <line f:pos="between" xml:id="lba"><handShift new="#g_bl"/><gap precision="medium" quantity="30" unit="chars"/></line>
                <line f:pos="over" xml:id="lbb"><handShift new="#g_t"/>Vom ernſten Warnegeiſt zu deinem Ohr getrieben!</line>
                <line f:pos="between" xml:id="lbc"><handShift new="#g_bl"/><gap precision="medium" quantity="14" unit="chars"/> zu dir getrieben</line>
                <line f:pos="over" xml:id="lbd"><handShift new="#g_t"/>Sein väterliches Herz von Sorge bangt um dich.</line>
                <line f:pos="between" xml:id="lbe"><handShift new="#g_bl"/>Sein väterliches Herz von Sorge bangt um dich</line>
                <line f:pos="over" rend="centered" xml:id="lbf"><handShift new="#g_t"/><hi rend="underline" hand="#g_t">Kayſer</hi></line>
                <line xml:id="lbg">Was haſt du bängliches zur frohen Stunde sprich!</line>
                <line rend="centered" xml:id="lbh"><handShift new="#g_bl"/>Kayſer</line>
                <line xml:id="lbi">Was haſt du bängliches zur frohen Stunde, ſprich.</line>
                <space dim="vertical" quantity="3.5" unit="lines"/>
                <line rend="indent-70" xml:id="lbj"><handShift new="#xx_bl_lat"/><fw type="shelfmark">30. 11</fw></line>
            </zone>
            <zone f:right-left="#mainzone" f:top="#Fuersten">
