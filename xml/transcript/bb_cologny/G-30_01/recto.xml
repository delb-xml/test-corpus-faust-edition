    <sourceDoc>
        <surface>
            <zone type="main" xml:id="mainzone">
                <space dim="vertical" quantity="1" unit="lines"/>
                <line rend="centered" xml:id="la"><handShift new="#g_t"/><retrace hand="#g_bl">C</retrace>anzler</line>
                <line xml:id="lb">Die höchſte Tugend, wie ein <f:overw>
                        <f:under>h</f:under>
                        <f:over><handShift new="#g_bl"/>H</f:over>
                    </f:overw><handShift new="#g_t"/>eiligen<handShift new="#g_bl"/>-<handShift new="#g_t"/>Schein<handShift new="#g_bl"/>,</line>
                <line xml:id="lc"><handShift new="#g_t"/>Umgiebt des Kayſers Haupt; nur er allein</line>
                <line xml:id="ld">Vermag ſie gültig auszuüben:</line>
                <line xml:id="le">Gerechtigkeit<f:overw>
                        <f:under>;</f:under>
                        <f:over>! –</f:over>
                    </f:overw><c xml:space="preserve"> </c>
                    <f:overw>
                        <f:under>w</f:under>
                        <f:over>W</f:over>
                    </f:overw>as alle <f:overw>
                        <f:under>m</f:under>
                        <f:over>M</f:over>
                    </f:overw>enſchen lieben<f:overw>
                        <f:under>.</f:under>
                        <f:over>,</f:over>
                    </f:overw></line>
                <line xml:id="lf">Was alle fordern, wünſchen, ſchwer entbehren,</line>
                <line f:left="#volcke" type="inter" xml:id="lg">es</line>
                <line xml:id="lh">Es liegt an ihm dem Volck’<mod rend="strikethrough">e</mod><anchor xml:id="volcke"/> zu gewähren.</line>
                <line f:left="#dem-geiste" type="inter" xml:id="li">Menſchengeiſt</line>
                <line xml:id="lj">Doch ach! was hilft dem<anchor xml:id="dem-geiste"/> <mod rend="strikethrough">Geiſte</mod> <mod rend="strikethrough">der</mod> Verſtand,</line>
                <line xml:id="lk">Dem Herzen Güte, Willigkeit der Hand,</line>
                <line type="inter" xml:id="lat">
                    <seg f:left="#durchaus">
                        <app>
                            <lem><handShift new="#g_bl"/>hin</lem>
                            <rdg resp="#wa-app"/>
                        </app>
                    </seg>
                    <seg>
                        <handShift new="#g_t"/>
                        <f:ins f:at="#innern" f:orient="left">Staate</f:ins>
                    </seg>
                </line>
                <line xml:id="wenns-wuethet">Wenns fieberhaft durch<anchor xml:id="durchaus"/><app>
                        <lem><mod rend="strikethrough" hand="#g_bl">aus</mod><!-- FIXME Rücknahme der Durchstreichung kodieren --></lem>
                        <rdg resp="#wa-app">aus</rdg>
                    </app> im <mod rend="strikethrough">innern</mod><anchor xml:id="innern"/> wüthet,</line>
                <line xml:id="ll">Und Übel ſich in Übeln überbrütet:</line>
                <line rend="centered" type="inter" xml:id="lm">
                    <g ref="#g_transp_1"/>
                </line>
                <line xml:id="ln"><handShift new="#g_bl"/><retrace hand="#g_tr"><g ref="#g_break"/></retrace><handShift new="#g_t"/>Der raubt ſich <f:overw>
                        <f:under>h</f:under>
                        <f:over>H</f:over>
                    </f:overw>eerden, der ein Weib,</line>
                <line xml:id="lo">Kelch, Kreuz und Leuchter vom Altare,</line>
                <line xml:id="lp">Berühmt ſich deſſen manche Jahre</line>
                <line f:left="#vollkommenem" type="inter" xml:id="lq">mit unverletztem</line>
                <line xml:id="lr">Mit heiler Haut, <anchor xml:id="vollkommenem"/><app>
                        <lem resp="#wa-app"><mod rend="strikethrough">vollkommnem</mod></lem>
                    </app> Leib.</line>
                <line type="inter" xml:id="ls"><seg f:left="#die-klaeger"><handShift new="#g_tr"/>Jetzt</seg>
                    <seg f:left="#Klaeger">2</seg>
                    <seg f:left="#draengen">1</seg></line>
                <line xml:id="lt"><handShift new="#g_t"/><anchor xml:id="die-klaeger"/><mod rend="strikethrough">Die</mod> Kl<anchor xml:id="Klaeger"/>äger drä<anchor xml:id="draengen"/>ngen ſich zur Halle,</line>
                <line f:left="#sitzt" type="inter" xml:id="lu">prunckt</line>
                <line xml:id="lv">Der Richter <anchor xml:id="sitzt"/><mod rend="strikethrough">ſitzt</mod> auf hohem Pfühl,</line>
                <line xml:id="lw">Indeſſen wogt, in grimmigem Schwalle,</line>
                <line xml:id="lx">Des Aufruhrs wachſendes Gewühl.</line>
                <line xml:id="ly">Der darf auf Schand und Frevel pochen,</line>
                <line xml:id="lz">Der auf Mitſchuldigſte ſich ſtüzt,</line>
                <line xml:id="laa">Und: <hi rend="underline"><f:overw>
                            <f:under>ſ</f:under>
                            <f:over>S</f:over>
                        </f:overw>chuldig</hi>! hör<f:overw>
                        <f:under>t</f:under>
                        <f:over>ſ</f:over>
                    </f:overw>t du ausgeſprochen</line>
                <line xml:id="lab">Wo Unſchuld nur ſich ſelber ſchüzt.</line>
                <line type="inter" xml:id="lad"><f:ins f:at="#will" f:orient="left" hand="#g_tr"><handShift new="#g_tr"/>ſich alle</f:ins></line>
                <line xml:id="lac"><handShift new="#g_t"/>So will<anchor xml:id="will"/>
                    <mod rend="strikethrough" hand="#g_tr">die</mod> Welt <mod rend="strikethrough" hand="#g_tr">ſich ſelbſt</mod> zerſtückeln,</line>
                <line xml:id="lae">Vernichtigen was ſich gebührt;</line>
                <line xml:id="laf">Wie ſoll ſich da der Sinn entwickeln</line>
                <line xml:id="lag">Der einzig uns zum Rechten führt.</line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line f:left="#und-selbst" type="inter" xml:id="lah">Zulezt</line>
                <line xml:id="lai"><anchor xml:id="und-selbst"/><mod rend="strikethrough">Und ſelbſt</mod> ein wohlgeſinnter Mann</line>
                <line f:left="#zum" type="inter" xml:id="laj">dem</line>
                <line xml:id="lak">Neigt ſich <anchor xml:id="zum"/><mod rend="strikethrough">zum</mod> Schmeichler, dem Beſtecher.</line>
                <line f:right-left="#helfen" type="inter" xml:id="lal">ſtra</line>
                <line xml:id="lam">Ein Richter der nicht <mod rend="strikethrough">hel</mod><anchor xml:id="helfen"/>fen kann</line>
                <line xml:id="lan">Geſellt ſich endlich zum Verbrecher.</line>
                <line type="inter" xml:id="lbc"><f:overw>
                        <f:under><g ref="#g_transp_1"/></f:under>
                        <f:over><handShift new="#g_tr"/><g ref="#g_transp_2"/></f:over>
                    </f:overw>
                </line>
                <line xml:id="lbd"><g ref="#g_break"/><anchor xml:id="Entschluesse"/><handShift new="#g_t"/>Entſchlüſſe ſind nicht zu vermeiden,</line>
                <line xml:id="lbe">Wenn alle ſchädigen, alle leiden</line>
                <line xml:id="lbf">Geht ſelbſt die Majeſtät zu Raub.</line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lbg"><f:overw>
                        <f:under><g ref="#g_transp_1"/></f:under>
                        <f:over><handShift new="#g_tr"/><g ref="#g_transp_2"/></f:over>
                    </f:overw><handShift new="#g_t"/>Ich mahlte ſchwarz doch dichtern Flor</line>
                <line xml:id="lbh">
                    <mod rend="strikethrough">D<app>
                            <lem><f:overw>
                                    <f:under>ie</f:under>
                                    <f:over>a</f:over>
                                </f:overw></lem>
                            <rdg resp="#wa-app">a</rdg>
                        </app>s Bild zu decken z<f:overw>
                            <f:over>o</f:over>
                            <f:over><handShift new="#g_bl"/>ö</f:over>
                        </f:overw><handShift new="#g_t"/>g ich vor</mod>
                </line>
                <line xml:id="lbi">Zög ich dem Bilde lieber vor. <space dim="horizontal" quantity="2" unit="chars"/><handShift new="#g_tr"/>(Pauſe)</line>
            </zone>
            <zone f:right="#mainzone" f:top="#mainzone">
                <line xml:id="lbj"><handShift new="#xx_bl_lat"/><note>1830, <abbr>S.</abbr> 76<gap quantity="1" unit="chars"/> <abbr>f</abbr></note></line>
                <line rend="right" xml:id="lao"><note><hi rend="underline"><abbr>L.</abbr> 8</hi></note></line>
            </zone>
            <zone f:right-left="#mainzone" f:top="#wenns-wuethet" rotate="90">
                <line xml:id="lap"><handShift new="#g_bl"/><unclear cert="high"><damage agent="pastedOver">Wer ſchaut hin</damage>ab</unclear> von dieſem hohen Raum,</line>
                <line xml:id="laq"><damage agent="pastedOver"><supplied evidence="internal">Ins weite Re</supplied></damage>ich, ihm ſcheint's ein ſchwerer Traum.</line>
                <line xml:id="lar"><damage agent="pastedOver"><supplied evidence="internal">Wo Mißgeſtalt</supplied></damage> <damage agent="rubbing">in Mißgeſtalten</damage> ſchaltet</line>
                <line type="inter" xml:id="lar1"><mod rend="strikethrough"><damage agent="rubbing"><unclear cert="high">Pauſe</unclear></damage></mod></line>
                <line xml:id="las"><damage agent="pastedOver"><supplied evidence="internal">Und eine Welt</supplied></damage> des Irrthums ſich entfaltet.<anchor xml:id="entfaltet"/></line>
            </zone>
            <surface type="patch">
                <zone f:right-left="#mainzone" f:top="#wenns-wuethet">
                    <line rend="centered" xml:id="lau"><handShift new="#g_t"/><g ref="#g_transp_1"/></line>
                    <line xml:id="lav"><mod rend="strikethrough" hand="#g_tr"><g ref="#g_break"/></mod>Wer ſchaut hinab von dieſem hohen Raum</line>
                    <line xml:id="law">In’s weite Reich, ihm ſcheint’s ein ſchwerer Traum;</line>
                    <line xml:id="lax">Wo Mißgeſtalt in Mißgeſtalten ſchaltet,</line>
                    <line type="inter" xml:id="lay"><handShift new="#g_tr"/><g ref="#g_transp_1"/></line>
                    <line xml:id="laz"><handShift new="#g_t"/>Und eine Welt des Irrthums ſich entfaltet.</line>
                    <line xml:id="lba"><handShift new="#g_tr"/>Das Ungeſetz geſetzlich über waltet, <space dim="horizontal" quantity="2" unit="chars"/><mod rend="strikethrough">(Pauſe)</mod></line>
                </zone>
                <zone f:right-left="#lba" f:top="#lba">
                    <line xml:id="lbb"><handShift new="#g_tr"/><g ref="#g_transp_1"/><space dim="horizontal" quantity="1" unit="chars"/></line>
                </zone>
            </surface>
        </surface>
    </sourceDoc>
