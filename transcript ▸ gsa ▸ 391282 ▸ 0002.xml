   <sourceDoc>
      <surface>
         <zone type="main">
            <metamark function="used" hand="#g_ro" spanTo="#alles"/>
            <line xml:id="la" rend="right">
               <fw type="folioNum">
                  <handShift new="#g_bl"/>
                  <retrace hand="#g_bl">33</retrace>.</fw>
            </line>
            <line xml:id="lb" rend="centered" f:pos="between">
               <handShift new="#g_bl"/>
               <unclear cert="high">Plutus Wagenlencker</unclear>
            </line>
            <line xml:id="lc" rend="indent-center-60" f:pos="over"><handShift new="#jo_t"/><hi rend="underline" hand="#jo_t">Plutus.</hi> (zum Lenker)</line>
            <line xml:id="ld">Nun biſt du los der allzuläſtigen Schwere,</line>
            <line xml:id="le">Biſt frey und frank, nun friſch zu deiner Sphä<mod rend="strikethrough">h</mod>re<f:overw>
                  <f:under>.</f:under>
                  <f:over><handShift new="#g_bl"/>!</f:over>
               </f:overw></line>
            <line xml:id="lf"><handShift new="#jo_t"/>Hier iſt ſie nicht<handShift new="#g_t"/>! <handShift new="#jo_t"/><f:overw>
                  <f:under>v</f:under>
                  <f:over><handShift new="#g_t"/>V</f:over>
               </f:overw><handShift new="#jo_t"/>erworren, ſchäckig, wild</line>
            <line xml:id="lg">Umdrängt uns hier ein frazzenhaft Gebild.</line>
            <line f:left="#wenn" xml:id="lh" type="inter"><handShift new="#g_t"/>wo</line>
            <line xml:id="li"><handShift new="#jo_t"/>Nur <anchor xml:id="wenn"/><mod rend="strikethrough" hand="#g_t">wenn</mod><anchor xml:id="anchor_269"/> du klar ins <f:overw>
                  <f:under>H</f:under>
                  <f:over><handShift new="#g_bl"/>h</f:over>
               </f:overw><handShift new="#jo_t"/>olde Klare ſchauſt,</line>
            <line xml:id="lj">Dir angehörſt und dir allein vertrauſt,</line>
            <line xml:id="lk">Dort<anchor xml:id="Dort"/> wo <mod rend="strikethrough" hand="#g_t">das</mod> Schöne<handShift new="#g_t"/><seg rend="inbetween">s</seg><handShift new="#jo_t"/> Gute<handShift new="#g_t"/><seg rend="inbetween">s</seg><handShift new="#jo_t"/> nur gefällt,</line>
            <line xml:id="ll" type="inter" f:left="#Dort"><handShift new="#g_t"/>hin</line>
            <line xml:id="lm"><handShift new="#g_bl"/><f:ins f:at="#Rufezeichen" f:orient="right" hand="#g_bl">–</f:ins></line>
            <line xml:id="ln"><handShift new="#jo_t"/>Zur Einſamkeit!<anchor xml:id="Rufezeichen"/> Da ſchaffe deine Welt.</line>
            <line xml:id="ln1" f:pos="between" rend="centered"><handShift new="#g_bl"/><unclear cert="high">Knabe Lenker</unclear></line>
            <line xml:id="lo" rend="centered" f:pos="over"><handShift new="#jo_t"/><hi rend="underline" hand="#jo_t">Knabe Lenker</hi>.</line>
            <line xml:id="lp">So acht ich mich als werthen Abgeſandten,</line>
            <line xml:id="lq">So lieb ich dich als nächſten Anverwandten.</line>
            <line xml:id="lr">Wo du verweilſt iſt Fülle, wo ich bin</line>
            <line xml:id="ls">Fühlt jeder ſich im herrlichſten Gewinn<retrace hand="#g_bl">;</retrace></line>
            <line xml:id="lt">Auch ſchwankt er oft im widerſinnigen Leben<handShift new="#g_bl"/>:</line>
            <line xml:id="lu"><handShift new="#jo_t"/>Soll er ſich dir ſoll er ſich mir ergeben<f:overw>
                  <f:under>.</f:under>
                  <f:over><handShift new="#g_bl"/>?</f:over>
               </f:overw></line>
            <line xml:id="lv"><handShift new="#g_t"/>Die Deinen freylich können müßig ruhn,</line>
            <line xml:id="lw">Doch wer mir folgt hat immer was zu thun.</line>
            <space dim="vertical" quantity="0.5" unit="lines"/>
            <line xml:id="lx"><handShift new="#jo_t"/>Nicht ins Geheim vollführ’ ich meine Thaten</line>
            <line xml:id="lx1" f:pos="between" rend="centered"><handShift new="#g_bl"/><gap quantity="10" unit="chars" precision="medium"/></line>
            <line xml:id="ly" f:pos="over"><handShift new="#jo_t"/>Ich athme nur und ſchon bin ich verrathen.</line>
            <line xml:id="ly1" f:pos="between" rend="centered"><handShift new="#g_bl"/><gap quantity="3" unit="chars" precision="medium"/></line>
            <line xml:id="lz" f:pos="over"><handShift new="#jo_t"/>So lebe wohl<handShift new="#g_bl"/>! <handShift new="#jo_t"/>du gönnſt mir ja mein Glück</line>
            <line f:left="#nur" xml:id="laa" type="inter"><handShift new="#g_bl"/>leis’</line>
            <line xml:id="lab"><handShift new="#jo_t"/>Doch lisple <anchor xml:id="nur"/><mod rend="strikethrough" hand="#g_bl">nur</mod> und gleich bin ich zurück.</line>
            <line xml:id="lac" rend="indent-70" type="inter"><handShift new="#g_t"/>(ab)</line>
            <line xml:id="lad" rend="centered"><handShift new="#jo_t"/><hi rend="underline" hand="#jo_t">Plutus</hi>.</line>
            <line xml:id="lae">Nun iſt es Zeit die Schätze zu entfeſſeln</line>
            <line xml:id="laf">Die Schlöſſer treff ich mit des Herolds Ruthe<handShift new="#g_bl"/>.</line>
            <line xml:id="lag"><handShift new="#jo_t"/>Es thut ſich auf<retrace hand="#g_t"><handShift new="#g_bl"/>!</retrace> <handShift new="#jo_t"/>ſchaut her<retrace hand="#g_t"><handShift new="#g_bl"/>!</retrace> <handShift new="#jo_t"/>in ehrnen Keſſeln</line>
            <line xml:id="lah">Entwickelt ſichs und <f:overw>
                  <f:under><mod rend="erase" hand="#g">W</mod></f:under>
                  <f:over><handShift new="#g_t"/>w</f:over>
               </f:overw><handShift new="#jo_t"/>al<f:overw>
                  <f:under><mod rend="strikethrough"><supplied evidence="internal">d</supplied></mod></f:under>
                  <f:over><handShift new="#g_t"/>l</f:over>
               </f:overw>t<f:grLine f:orient="vert" hand="#g_t" f:style="linear"/><handShift new="#jo_t"/>von goldnem Blute<handShift new="#g_t"/>,</line>
            <line xml:id="lai"><handShift new="#jo_t"/>Zunächſt der Schmuck von Kronen Ketten Ringen</line>
            <line xml:id="laj" type="inter" f:left="#lak"><handShift new="#g_t"/>Es</line>
            <line xml:id="lak"><handShift new="#jo_t"/><mod rend="strikethrough" hand="#g_t">Sie</mod> ſchwillt und droht ihn ſchmelzend zu verſchlingen.</line>
            <anchor xml:id="alles"/>
