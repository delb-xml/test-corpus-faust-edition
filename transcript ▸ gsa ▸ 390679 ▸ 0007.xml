    <sourceDoc>
        <surface>
            <zone type="main" xml:id="mainzone">
                <line xml:id="la"><handShift new="#g_bl"/>·</line>
                <line xml:id="lb">·</line>
                <line xml:id="lc">·</line>
                <line xml:id="ld">·</line>
                <line xml:id="le">·</line>
                <line xml:id="lf">·</line>
                <line xml:id="lg">·</line>
                <line xml:id="lh">·</line>
                <line xml:id="li"><handShift new="#g_t"/>Ich wollte, frey umher zu ſchauen,</line>
                <line xml:id="lj">Ein Lug-‿in’s-Land, ein <app>
                        <lem>Höchſtes</lem>
                        <rdg resp="#landeck">höchſtes</rdg>
                        <rdg resp="#es">höchſtes</rdg>
                    </app> bauen,</line>
                <line xml:id="lk">Dadroben hätt’ ich weite Bahn</line>
                <line xml:id="ll">Zu ſehn was alles ich gethan</line>
                <line xml:id="lm">Zu überſchaun, mit <app>
                        <lem>Einem Blick,</lem>
                        <rdg resp="#landeck">einem Blick</rdg>
                    </app></line>
                <line xml:id="ln">Des Menſchen-Geiſtes Meiſterſtück</line>
                <line xml:id="lo">Bethätigend, mit klugem Sinn,</line>
                <line xml:id="lp">Der Völker breiten Wohngewinn.</line>
                <line xml:id="lq">So ſind am Härtſten wir gequält</line>
                <line xml:id="lr">Im Reichthum fühlend was uns fehlt.</line>
                <line xml:id="ls">Des Glöckchens Klang, der Linden Duft</line>
                <line type="inter" xml:id="lt">
                    <handShift new="#g_bl"/>
                    <seg f:left="#seg_1">
                        <retrace hand="#g_t">Umfängt</retrace>
                    </seg>
                    <seg f:left="#seg_2">
                        <f:overw>
                            <f:under><gap precision="medium" quantity="3" unit="chars"/></f:under>
                            <f:over><handShift new="#g_t"/>in Kirch</f:over>
                        </f:overw><c xml:space="preserve"> </c><handShift new="#g_bl"/><retrace hand="#g_t">und</retrace></seg>
                </line>
                <line xml:id="lu"><handShift new="#g_t"/><anchor xml:id="seg_1"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#g_bl">Erdr<orig>u</orig>cken</mod></mod> mich wie <anchor xml:id="seg_2"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#g_bl">in</mod></mod>
                    <mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#g_bl">der</mod></mod> Gruft.</line>
                <line f:left="#seg_2" xml:id="lad" type="inter"><handShift new="#g_bl"/>Kirch<gap precision="medium" quantity="3" unit="chars"/></line>
                <line xml:id="lv"><handShift new="#g_t"/>Des allgewaltigen <app>
                        <lem resp="#es">Willes</lem>
                        <rdg resp="#landeck">Willens</rdg>
                    </app> Kühr</line>
                <line xml:id="lw">Bricht ſich an dieſem Sande <app>
                        <lem>Hier.</lem>
                        <rdg resp="#landeck">hier.</rdg>
                    </app></line>
                <line xml:id="lx">Ja! ſchaff<seg rend="inbetween">t</seg> ich mir es vom Gemüthe,</line>
                <line xml:id="ly">Das Glöcklein läutet und ich wüthe.</line>
                <line rend="centered" xml:id="lz">
                    <abbr>Meph.</abbr>
                </line>
                <line type="inter" xml:id="laa"><seg f:left="#seg_3">Natürlich!</seg><f:ins f:at="#ins_1" f:orient="left">ein Hauptv</f:ins></line>
                <line xml:id="lab"><anchor xml:id="seg_3"/><mod rend="strikethrough" hand="#g_t">Mich</mod>
                    <mod rend="strikethrough" hand="#g_t">wundert</mod>
                    <mod rend="strikethrough" hand="#g_t">nicht</mod> daß <mod rend="strikethrough" hand="#g_t">der</mod>
                    <anchor xml:id="ins_1"/><mod rend="strikethrough">V</mod>erdruß</line>
                <line xml:id="lac">Das Leben dir vergällen muß.</line>
            </zone>
            <zone f:right-left="#lq" f:top="#lq">
                <line><handShift new="#g_t"/><g ref="#g_break"/></line>
            </zone>
            <surface type="patch">
                <zone f:bottom-top="#Hauptzone_9alpha" f:right-left="#Hauptzone_9alpha">
                    <line><handShift new="#xx_bl"/><fw type="folioNum">9<hi rend="sup">α</hi></fw></line>
                </zone>
                <zone f:bottom="#ll" f:left="#mainzone" xml:id="Hauptzone_9alpha">
                    <line rend="indent-30" xml:id="Mephisto"><handShift new="#g_t"/><abbr><hi hand="#g_t" rend="underline">Mephiſto</hi>.</abbr><handShift new="#g_bl"/>(zu Fauſt)</line>
                    <line xml:id="Mit"><handShift new="#g_t"/>Mit ernſter Stirn, mit düſtrem Blick,</line>
                    <line>Vernimmſt du dein erhaben Glück.</line>
                    <line>Die hohe Weisheit wird gekrönt,</line>
                    <line>Das Ufer iſt dem Meer verſöhnt,</line>
                    <line>Vom Ufer nimmt, zu raſcher Bahn,</line>
                    <line>Das Meer die Schiffe willig an;</line>
                    <line type="inter"><f:ins f:at="#du" f:orient="left">hier</f:ins></line>
                    <line>So ſprich daß hier, <mod rend="strikethrough" hand="#g_t"><app>
                                <lem resp="#es">du</lem>
                                <rdg resp="#landeck">da</rdg>
                            </app></mod><anchor xml:id="du"/> vom Pallaſt</line>
                    <line>Dein Arm die ganze Welt umfaßt.</line>
                    <line>Von dieſer Stelle ging es aus,</line>
                    <line>Hier ſtand das erſte Breter Haus,</line>
                    <line>Ein Gräbchen ward <app>
                            <lem xml:space="preserve">hinab geri<seg rend="inbetween">t</seg>zt</lem>
                            <rdg resp="#landeck">hinabgeri<seg rend="inbetween">t</seg>zt</rdg>
                        </app></line>
                    <line>Wo jetzt das Ruder emſig ſpritzt.</line>
                    <line>Dein hoher Sinn, der Deinen Fleiß,</line>
                    <line>Erwarb des Meers der Erde Preiß</line>
                    <line>Von hier<f:overw>
                            <f:under><mod rend="strikethrough">!</mod></f:under>
                            <f:over><handShift new="#g_bl"/><retrace hand="#g_t">a</retrace></f:over>
                        </f:overw><retrace hand="#g_t">us –</retrace></line>
                    <line rend="centered">
                        <handShift new="#g_t"/>
                        <hi hand="#g_t" rend="underline">Fauſt</hi>
                    </line>
                    <line type="inter">
                        <handShift new="#g_bl"/>
                        <retrace hand="#g_t"><f:ins f:at="#Mit_dem" f:orient="right">Das</f:ins></retrace>
                    </line>
                    <line rend="indent-30"><handShift new="#g_t"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#g_bl">Mit dem</mod></mod><anchor xml:id="Mit_dem"/> verfluchte<mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#g_bl">n</mod></mod><c xml:space="preserve"> </c><app>
                            <lem resp="#landeck"><hi hand="#g_t" rend="underline">Hier</hi>!</lem>
                            <rdg resp="#es"><hi hand="#g_t" rend="underline">hier</hi>!</rdg>
                        </app></line>
                    <line>Das eben leidig laſtets mir.</line>
                    <line f:left="#alt" type="inter"><f:overw>
                            <f:under>v</f:under>
                            <f:over>V</f:over>
                        </f:overw>iel</line>
                    <line>Dir <anchor xml:id="alt"/><mod rend="strikethrough" hand="#g_t">alt</mod>gewandtem muß ich’s ſagen</line>
                    <line>Mir giebts im Herzen Stich um Stich.</line>
                    <line>Mir iſt’s unm<orig>o</orig>glich zu ertragen!</line>
                    <line>Und wie ich’s ſage ſchäm ich mich.</line>
                    <line>Die Alten droben ſollten weichen,</line>
                    <line>Die Linden wünſcht ich mir zum Si<retrace>t</retrace>z,</line>
                    <line>Die wenig Bäume, nicht mein eigen,</line>
                    <line f:left="#der" type="inter">n</line>
                    <line>Verderben mir de<anchor xml:id="der"/><mod rend="strikethrough" hand="#g_t">r</mod> Welt Beſitz.</line>
                    <line>Dort wollt ich, weit umher zu ſchauen,</line>
                    <line>Von Aſt zu Aſt Gerüſte bauen,</line>
                    <line>Dem Blick eröffnen weite Bahn,</line>
                    <line>Zu ſehn was alles ich gethan.</line>
                </zone>
            </surface>
