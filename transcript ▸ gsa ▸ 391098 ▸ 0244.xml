        <surface>
            <zone f:bottom-top="#mainzone" f:right-left="#mainzone">
                <line xml:id="lb">
                    <fw type="pageNum"><handShift new="#sc_bl"/>36</fw><space dim="horizontal" quantity="4" unit="chars"/>
                </line>
