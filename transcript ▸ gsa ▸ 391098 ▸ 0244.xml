            <zone type="main" xml:id="mainzone">
                <line rend="centered" xml:id="la">
                    <handShift new="#st_t"/>
                    <hi rend="underline">Chor</hi>
                </line>
                <line xml:id="lc">Aber wir?<anchor xml:id="anchor1"/></line>
                <line rend="centered" xml:id="ld">
                    <abbr><hi rend="underline">Phork</hi>:</abbr>
                </line>
                <line f:left="#anchor1" xml:id="le">Ihr wißt es deutlich, ſeht vor Augen ihren
                    Tod,</line>
                <line xml:id="lf">Merkt den eurigen da drinne; nein zu helfen iſt euch
                    nicht.</line>
                <line rend="centered" xml:id="lg">(Pauſe)</line>
                <line rend="centered" xml:id="lh"><hi rend="underline">Helena</hi>.</line>
                <line xml:id="li">Ich ſann mir aus das Nächſte was ich wagen darf.</line>
                <line xml:id="lj">Ein Widerdämon biſt du<f:overw>
                        <f:under><f:overw>
                                <f:under>!</f:under>
                                <f:over>.</f:over>
                            </f:overw></f:under>
                    <f:over><handShift new="#go_bl"/>,</f:over>
                    </f:overw><handShift new="#st_t"/> das empfind’ ich wohl,</line>
                <line xml:id="lk">Und fürchte, Gutes wendeſt du zum Böſen um.</line>
                <line xml:id="ll">Vor allem aber folgen will ich dir zur Burg;</line>
                <line type="inter" xml:id="lm"><handShift new="#g_t"/><f:ins f:at="#dabey" f:orient="right" hand="#g_t">i</f:ins></line>
                <line xml:id="Das"><handShift new="#st_t"/>Das andre wei<f:overw>
                        <f:under><mod rend="vertical strikethrough" hand="#go_bl">s</mod></f:under>
                        <f:over><handShift new="#g_t"/>ß</f:over>
                    </f:overw><handShift new="#st_t"/> ich; was die Königin dabe<mod rend="vertical strikethrough" hand="#g_t"><mod rend="vertical strikethrough" hand="#go_bl">y</mod></mod><anchor xml:id="dabey"/></line>
                <line xml:id="ln">In tiefe<f:overw>
                        <f:under>n</f:under>
                        <f:over><handShift new="#g_t"/>m</f:over>
                    </f:overw><handShift new="#st_t"/> Buſen geheimnißvoll verbergen mag,</line>
                <line xml:id="lo">Sey jedem unzugänglich. Alte! geh voran.</line>
                <line rend="centered" xml:id="lp">
                    <hi rend="underline">Chor.</hi>
                </line>
                <line rend="indent-10" xml:id="lq">O wie gern gehen wir hin,</line>
                <line rend="indent-10" xml:id="lr">Eilenden Fußes;</line>
                <line rend="indent-10" xml:id="ls">Hinter uns Tod,</line>
                <line rend="indent-10" xml:id="lt">Vor uns abermals</line>
                <line rend="indent-10" xml:id="lu">Ragender Veſte</line>
                <line rend="indent-10" xml:id="lv">Unzugängliche Mauer.</line>
                <line rend="indent-10" xml:id="lw">Schütze ſie eben ſo gut</line>
                <line rend="indent-10" xml:id="Eben">Eben wie Ilio<mod rend="vertical strikethrough" hand="#g_t"><mod rend="vertical strikethrough" hand="#go_bl">n</mod></mod>s
                    Burg,</line>
                <line rend="indent-10" xml:id="lx">Die doch endlich nur</line>
                <line rend="indent-10" xml:id="ly">Niederträchtiger Liſt erlag.</line>
                <line rend="indent-20" xml:id="lz">(Nebel verbreiten ſich, umhüllen den
                    Hinter-</line>
                <line rend="indent-20" xml:id="laa">grund, auch die Nähe, nach
                    Belieben)</line>
            </zone>
