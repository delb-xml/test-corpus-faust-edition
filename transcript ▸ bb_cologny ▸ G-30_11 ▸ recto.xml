    <sourceDoc>
        <surface>
            <zone type="main">
                <line rend="right" xml:id="la"><handShift new="#xx_bl_lat"/><note>1832 <abbr>S.</abbr> 291.</note></line><!-- Ottilie? -->
                <line rend="right" xml:id="lb"><note><hi hand="#xx_bl_lat" rend="underline"><abbr>L.</abbr> 204.</hi></note></line>
                <line rend="centered" xml:id="lc"><handShift new="#g_t"/><hi hand="#g_t" rend="underline">Kayſer</hi></line>
                <line type="inter" xml:id="ld">Wenn</line>
                <line xml:id="le"><mod rend="strikethrough" hand="#g_t">Wie</mod> ein Gewölbe ſich dem Schlußſtein anvertraut</line>
                <line xml:id="lf">Dann iſt’s mit Sicherheit für ewige Zeit erbaut.</line>
                <line xml:id="lg">Du ſiehſt Vier Fürſten da! Wir haben erſt erörtert,</line>
                <line xml:id="lh">Was den Bedarf zunächſt von Haus und Hof bef<orig>o</orig>rdert.</line>
                <line xml:id="li">Nun aber was das Reich in ſeinem Ganzen hegt,</line>
                <line xml:id="lj">Sey mit Gewicht und Kraft der Fünfzahl auferlegt.</line>
                <space dim="vertical" quantity="1.5" unit="lines"/>
                <line xml:id="lk">An Ländern ſollen ſie vor allen andern glänzen,</line>
                <line f:pos="between" xml:id="ll"><handShift new="#g_bl"/>An L<unclear cert="high">än</unclear>d<unclear cert="high">ern</unclear> ſ<unclear cert="high">ollen</unclear> ſie <unclear cert="high">vor allen andern glänzen</unclear></line>
                <line f:pos="over" xml:id="lm"><handShift new="#g_t"/>Deßhalb erweitre ich gleich jetzt des Beſitzthums</line>
                <line rend="indent-70" xml:id="ln">Gränzen,</line>
                <line f:pos="between" xml:id="lo"><handShift new="#g_bl"/>Deshalb erweitre ich gleich jetzt des <unclear cert="high">Beſizthums</unclear></line>
                <line f:pos="over" xml:id="lp"><handShift new="#g_t"/>Vom Erbtheil jener die ſich von uns abgewandt.</line>
                <line f:pos="between" rend="indent-70" xml:id="lq"><handShift new="#g_bl"/><unclear cert="high">Gränzen</unclear></line>
                <line f:pos="over" xml:id="lr"><handShift new="#g_t"/>Euch Treuen ſprech ich zu ſo manches ſchöne Land,</line>
                <line f:pos="between" xml:id="ls"><handShift new="#g_bl"/>Vom Erbtheil jener die von uns ſich abgewandt</line>
                <line f:pos="over" xml:id="lt"><handShift new="#g_t"/>Zugleich das hohe Recht <f:overw><f:under>E</f:under>
                    <f:over>e</f:over>
                </f:overw>uch nach Gelegenheiten.</line>
                <line f:pos="between" xml:id="lu"><handShift new="#g_bl"/>Euch Treuen ſprech ich zu ſo manches ſchöne Land,</line>
                <line f:pos="over" xml:id="lv"><handShift new="#g_t"/>Durch Anfall, Kauf und Tauſch in’s <f:overw><f:under>w</f:under>
                    <f:over>W</f:over>
                </f:overw>eitere zu ver-</line>
                <line f:pos="over" rend="indent-70" xml:id="lw">breiten.</line>
                <line f:pos="between" xml:id="lx"><handShift new="#g_bl"/>Zugleich das hohe Recht Euch nach <unclear cert="high">Gelegenheiten</unclear></line>
                <line f:pos="over" xml:id="ly"><handShift new="#g_t"/>Sodann ſey Euch aus zu üben ungeſtört,</line>
                <line f:pos="between" xml:id="lz"><handShift new="#g_bl"/>Durch <unclear cert="low">Anfall, Kauf und Tauſch</unclear> in Weit<unclear cert="low">res</unclear> zu verbreiten</line>
                <line f:pos="over" xml:id="laa"><handShift new="#g_t"/>Was von Gerechtſamen dem Landesherrn</line>
                <line rend="indent-70" xml:id="lab">geh<orig>o</orig>rt.</line>
                <line f:pos="between" xml:id="lac"><handShift new="#g_bl"/>Sodann ſey euch <unclear cert="high">das Recht zu</unclear> üben unſtört</line>
                <line f:pos="over" xml:id="lad"><handShift new="#g_t"/>Als Richter werdet ihr die Endurtheile f<orig>a</orig>llen.</line>
                <line f:pos="between" xml:id="lae"><handShift new="#g_bl"/>Was von Gerechtſamen <unclear cert="low"><abbr>d</abbr> <abbr>Landesh</abbr></unclear> <unclear cert="high">gehört</unclear></line>
                <line f:pos="over" xml:id="laf"><handShift new="#g_t"/>Berufung gelte nicht von E<unclear cert="high">uer</unclear>n höchſten Stellen,</line>
                <line f:pos="between" xml:id="lag"><handShift new="#g_bl"/>Als Richter werdet ihr <unclear cert="low">das</unclear> Endurtheil fällen</line>
                <line f:pos="over" xml:id="lah"><handShift new="#g_t"/>Dann Steuer Zins und Beet, Lehn und Geleit und Zoll</line>
                <line f:pos="between" xml:id="lai"><handShift new="#g_bl"/>Berufung <unclear cert="high">gelte</unclear> nicht von <unclear cert="high">Euern</unclear> höchſten St<unclear cert="high">tellen</unclear></line>
                <line f:pos="over" xml:id="laj"><handShift new="#g_t"/>Das Berg- und Münzregal Euch angeh<orig>o</orig>ren ſoll.</line>
                <line f:pos="between" xml:id="lak"><handShift new="#g_bl"/>Dann Steuer Zins <unclear cert="high">und Beet Lehn <abbr>u</abbr></unclear> Geleit und Zoll</line>
                <line f:pos="over" xml:id="lal"><handShift new="#g_t"/>Denn meine Danckbarkeit vollgültig zu erproben,</line>
                <line f:pos="between" xml:id="lam"><handShift new="#g_bl"/>Auch Münz<unclear cert="high">- <abbr>u</abbr> Berg</unclear> <unclear cert="low">Regal</unclear> euch angeh<orig>o</orig>ren ſoll</line>
                <line f:pos="over" xml:id="lan"><handShift new="#g_t"/>Hab ich Euch ganz zunächſt der Majeſt<orig>a</orig>t erhoben.</line>
                <line f:pos="between" xml:id="lao"><handShift new="#g_bl"/><unclear cert="high">Wenn</unclear> Eure Dan<unclear cert="high">ckbarkeit</unclear> <gap precision="medium" quantity="3" unit="chars"/> <unclear cert="high">ganz</unclear> zu erproben</line>
                <line f:pos="over" rend="centered" xml:id="lap"><handShift new="#g_t"/><hi hand="#g_t" rend="underline"><abbr>Erzk.</abbr></hi></line>
                <line f:pos="between" xml:id="laq"><handShift new="#g_bl"/>Hab ich euch ganz zunächſt <unclear cert="low">der Majeſtät erhoben</unclear></line>
                <line f:pos="over" xml:id="lar"><handShift new="#g_t"/>Im Nahmen aller ſey dir tiefſter Danck gebracht!</line>
                <line f:pos="between" rend="centered" xml:id="las"><handShift new="#g_bl"/><gap precision="medium" quantity="5" unit="chars"/></line>
                <line f:pos="over" xml:id="lat"><handShift new="#g_t"/>Du machſt uns ſtarck und feſt und ſtärckeſt deine Macht.</line>
                <line f:pos="between" xml:id="lau"><handShift new="#g_bl"/>Im Nahmen aller ſ<unclear cert="high">ey dir tiefſter</unclear> Danck gebracht</line>
                <line f:pos="over" rend="centered" xml:id="lav"><handShift new="#g_t"/><hi hand="#g_t" rend="underline">Kayſer</hi></line>
                <line f:pos="between" xml:id="law"><handShift new="#g_bl"/><unclear cert="low">Du machſt uns ſtark und</unclear> feſt und ſtärkeſt deine Macht</line>
                <line f:pos="over" xml:id="lax"><handShift new="#g_t"/>Euch fünfen will ich noch erhöhtere Würde geben.</line>
                <line f:pos="between" rend="centered" xml:id="lay"><handShift new="#g_bl"/>K<unclear cert="low">ayſer</unclear></line>
                <line f:pos="over" xml:id="laz"><handShift new="#g_t"/>Noch leb ich meinem Reich und habe Luſt zu leben;</line>
                <line f:pos="over" xml:id="lba">Doch hoher Ahnen Kette zieht bedächtigen Blick,</line>
                <line f:pos="between" xml:id="lbb"><handShift new="#g_bl"/>Euch fünfen will ich noch erhöhtere Würde geben</line>
                <line f:pos="over" xml:id="lbc"><handShift new="#g_t"/>Aus raſcher Strebſamkeit ins Drohende zurück.</line>
                <line f:pos="between" xml:id="lbd"><handShift new="#g_bl"/>Noch leb ich meinem Reich und habe Luſt zu leben</line>
                <line f:pos="over" xml:id="lbe"><handShift new="#g_t"/>Auch werd ich, ſeiner Zeit, mich von den Theuren</line>
                <line f:pos="over" rend="indent-70" xml:id="lbf"><handShift new="#g_t"/>trennen.</line>
                <line f:pos="between" xml:id="lbg"><handShift new="#g_bl"/><unclear cert="low">Doch hoher Ahnen Kette zieht</unclear> bed<orig>a</orig>chtigen Blick</line>
                <line f:pos="over" xml:id="lbh"><handShift new="#g_t"/>Dann ſey es Eure Pflicht den Folger zu ernennen.</line>
                <line f:pos="between" xml:id="lbi"><handShift new="#g_bl"/><gap precision="medium" quantity="24" unit="chars"/><unclear cert="high">zurück</unclear></line><!-- Aus rascher... -->
                <line f:pos="over" xml:id="lbj"><handShift new="#g_t"/>Gekrönt erheb<mod rend="strikethrough">e</mod>t ihn hoch auf heiligem Altar</line>
                <line f:pos="over" xml:id="lbk"><handShift new="#g_t"/>Und friedlich end<f:overw>
                        <f:under>r</f:under>
                        <f:over>e</f:over>
                    </f:overw> das was jetzt ſo ſtürmiſch war.</line>
                <line xml:id="lbp"><handShift new="#g_bl"/>Auch werd ich ſeiner Zeit mich von den theuren trennen</line>
                <line xml:id="lbq">Dann ſey es Eure Pflicht den Folger zu benennen</line>
                <line xml:id="lbs" type="inter"><f:ins f:at="#sollt" f:orient="right" hand="#g_bl">gekr<orig>o</orig>nt</f:ins></line>
                <line xml:id="lbr">Dann <unclear cert="high">hebt</unclear><anchor xml:id="sollt"/> auf den Altar</line>
                <line xml:id="lbo"><mod rend="strikethrough" hand="#g_bl">Und friedhaft</mod> <unclear cert="high">ſo ſch<damage agent="glue">lieſſ</damage>et</unclear> fried<gap unit="chars" quantity="4" precision="medium"></gap> was jetzt</line>
                <!-- mit Frau Moering am 9.9. intensiv diskutiert -->
                <line rend="indent-60" xml:id="lbn">ſo ſt<orig>u</orig>rmiſch war</line>
                <line rend="centered" xml:id="lbm"><mod rend="strikethrough" hand="#g_bl">(</mod>Die Fürſten<!-- besser als "Fünfe" WA --> <seg rend="inbetween">(</seg>neigen<!-- mit WA besser als "entfernen" --> ſich)</line><!-- h und schließende Klammer in einem Zug geschrieben -->
                <space dim="vertical" quantity="1" unit="lines"/>
                <line rend="right" xml:id="lbl"><fw><handShift new="#sc_bl_lat"/><num>VII</num><hi rend="sup">b</hi></fw></line><!-- folioNum? auf jeden Fall handelt es sich hier um das vierte Blatt des Alexandriner-Mundums -->
            </zone>
        </surface>
