    <sourceDoc>
        <surface>
            <zone type="main" xml:id="Hauptzone">
                <metamark function="used" hand="#g_bl" spanTo="#geformter"/>
                <line f:left="#guenstigt" xml:id="la" type="inter"><handShift new="#g_bl"/>gnadigt ſonder</line>
                <line xml:id="lb">Du aber, hochbe<anchor xml:id="guenstigt"/><mod rend="strikethrough" hand="#g_bl">günſtigt</mod>
                    <mod rend="strikethrough" hand="#g_bl">auſſer</mod> Maas und Ziel,</line>
                <line xml:id="lc">
                    <mod rend="strikethrough" hand="#g_bl">Lieb und Beg<f:overw>
                            <f:under>ie</f:under>
                            <f:over>e</f:over>
                        </f:overw>hr erregend all dein Leben</mod>
                </line>
                <line xml:id="ld">In langer Lebensreihe <f:overw>
                        <f:under>ſahſt nur Lieb<mod rend="strikethrough" hand="#g_bl">g<unclear cert="low">eſtalten</unclear></mod></f:under>
                        <f:over><handShift new="#g_t"/>Liebesbrünſtige ſahſt du nur,</f:over>
                    </f:overw></line>
                <line xml:id="le"><handShift new="#g_bl"/>Entz<orig>u</orig>ndete zum <f:overw>
                        <f:under>hohen</f:under>
                    <f:over>kühn</f:over>
                </f:overw>ſten Wag<f:overw>
                        <f:under>niß</f:under>
                        <f:over><handShift new="#g_t"/>eſtück</f:over>
                    </f:overw>
                    <handShift new="#g_bl"/>jeder Art</line>
                <line f:left="#zur-Lust" xml:id="lf" type="inter"><handShift new="#g_t"/>ein Begehrender</line>
                <line xml:id="lg"><handShift new="#g_bl"/>Schon Theſeus haſchte fr<orig>u</orig>h dich <anchor xml:id="zur-Lust"/><mod rend="strikethrough" hand="#g_bl">zur Luſt</mod><handShift new="#g_t"/>,</line>
                <line xml:id="lh" type="inter">
                    <handShift new="#g_bl"/>
                    <f:ins f:at="#Herkules" f:orient="right" hand="#g_bl"><mod rend="strikethrough" hand="#g_bl">jedoch</mod></f:ins>
                </line>
                <line xml:id="li">So ſtarck wie Herkules,<anchor xml:id="Herkules"/>
                    <anchor xml:id="ein-schoenrer"/><c xml:space="preserve"> </c><mod rend="strikethrough" hand="#g_bl">ein ſchönrer</mod> Mann</line>
                <line f:left="#ein-schoenrer" xml:id="lj" type="inter">ein ſch<orig>o</orig>n geformter</line>
                <anchor xml:id="geformter"/>
                <line xml:id="lk" rend="centered">
                    <handShift new="#jo_t_lat"/>
                    <num>II.</num>
                </line>
                <space dim="vertical" quantity="1" unit="lines"/>
                <!-- Neugriechisch-epirotische Heldenlieder -->
                <line xml:id="ll">Schwarzes Fahrzeug theilt die Welle,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>n</f:under>
                        <f:over><handShift new="#g_tr_lat"/>N</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>ächst der Küste von Kaſsandra,</line>
                <line xml:id="lm"><orig>Ue</orig>ber ihm die schwarzen Segel,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>ü</f:under>
                        <f:over><handShift new="#g_tr_lat"/>Ü</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>ber ihnen Himmels-Bläue.</line>
                <line xml:id="ln">Kom̄t ein Türken-Schiff entgegen,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/> Scharlach Wimpel wehen glänzend</line>
                <line xml:id="Streiche"><handShift new="#g_t_lat"/>„<handShift new="#jo_t_lat"/>Streich<mod rend="vertical strikethrough" hand="#g_t_lat">e</mod> die Segel unverzüglich<handShift new="#g_tr_lat"/>,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <handShift new="#jo_t_lat"/><f:overw>
                        <f:under>n</f:under>
                        <f:over><handShift new="#g_tr_lat"/>N</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>ieder laſs<mod rend="strikethrough" hand="#g_t_lat"><mod rend="strikethrough" hand="#ri_bl_lat">e</mod></mod> die Segel du<handShift new="#ri_bl_lat"/><retrace hand="#g_t_lat">!</retrace><handShift new="#g_t_lat"/>“ <handShift new="#ri_bl_lat"/><retrace hand="#g_t_lat">–</retrace></line>
                <line xml:id="lo"><handShift new="#jo_t_lat"/>Nein ich streiche nicht die Segel<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <handShift new="#jo_t_lat"/><f:overw>
                        <f:under>n</f:under>
                        <f:over><handShift new="#g_tr_lat"/>N</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>immer laſs ich sie herab</line>
                <line xml:id="lp">Droht ihr doch als wär’ ich Bräutchen,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/> Bräutchen das zu schrecken ist;</line>
                <line f:left="#vallas" xml:id="lq" type="inter"><handShift new="#g_t_lat"/>v</line>
                <line xml:id="Jannis"><handShift new="#jo_t_lat"/>Jannis bin ich, Sohn des Stada,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/> Eidam des Buko<anchor xml:id="vallas"/><mod rend="vertical strikethrough" hand="#g_t_lat"><mod rend="vertical strikethrough" hand="#ri_bl_lat">b</mod></mod>a<mod rend="strikethrough" hand="#g_t_lat"><mod rend="strikethrough" hand="#ri_bl_lat">l</mod></mod>las.</line>
                <line xml:id="lr">Frisch Gesellen, frisch zur Arbeit!<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>a</f:under>
                        <f:over><handShift new="#g_tr_lat"/>A</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>uf zum Vor<f:overw>
                        <f:under>t</f:under>
                        <f:over>d</f:over>
                    </f:overw>ertheil des Schiffes.</line>
                <line xml:id="ls">Türkenblut ist zu vergieſsen,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>s</f:under>
                        <f:over><handShift new="#g_tr_lat"/>S</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>chont nicht der Ungläubigen.</line>
                <line xml:id="Und-mit">Und mit einer klugen Wendung<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>b</f:under>
                        <f:over><handShift new="#g_tr_lat"/>B</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>eut das Türkenschiff die Spitz<anchor xml:id="Spitz"/><mod rend="strikethrough" hand="#g_t_lat">’</mod>
                    <mod rend="strikethrough" hand="#g_tr_lat"><mod rend="strikethrough" hand="#g_t_lat"><mod rend="strikethrough" hand="#ri_bl_lat">ihm</mod></mod></mod>;</line>
                <line f:left="#Spitz" xml:id="lt" type="inter">
                    <handShift new="#ri_bl_lat"/>
                    <retrace hand="#g_t_lat">e</retrace>
                </line>
                <line xml:id="Jannis-aber"><handShift new="#jo_t_lat"/>Jannis aber schwingt hinauf sich,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>m</f:under>
                        <f:over><handShift new="#g_tr_lat"/>M</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>it dem S<orig>ae</orig>bel in der Faust,</line>
                <line xml:id="lu">Das Gebälke trieft vom Blute<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>u</f:under>
                        <f:over><handShift new="#g_tr_lat"/>U</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>nd geröthet sind die Wellen.</line>
                <line xml:id="lv">Allah! Allah! schreyen um Gnade<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>d</f:under>
                        <f:over><handShift new="#g_tr_lat"/>D</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>ie Ungläubigen auf den Knieen.</line>
                <line xml:id="lw">Traurig Leben! ruft der Sieger,<f:grLine f:orient="vert" f:style="linear" hand="#g_tr_lat"/>
                    <f:overw>
                        <f:under>b</f:under>
                        <f:over><handShift new="#g_tr_lat"/>B</f:over>
                    </f:overw><handShift new="#jo_t_lat"/>leibe den Besiegten nun.</line>
                <metamark function="used" hand="#g_bl" spanTo="#Achilles"/>
                <line xml:id="lx" type="inter"><handShift new="#g_bl"/><f:ins f:at="#Als" f:orient="right" hand="#g_bl">Ergriff dann kindiſch das</f:ins></line>
                <line xml:id="ly"><mod rend="strikethrough" hand="#g_bl">Als</mod><anchor xml:id="Als"/> zehenj<orig>a</orig>hrigs Reh <mod rend="strikethrough" hand="#g_bl">entführt er mich</mod></line>
                <line f:left="#mich" xml:id="lz" type="inter">es</line>
                <line f:pos="between" rend="centered"><f:grBrace f:orient="horiz-bottom" f:style="curly" hand="#jo_t_lat"/></line>
                <line f:pos="over" xml:id="laa">Und zur Verwahrung gab er <anchor xml:id="mich"/><mod rend="strikethrough" hand="#g_bl">mich</mod> nach</line>
                <line xml:id="lab" rend="indent-50">Attika.</line>
                <line xml:id="lac" rend="centered"><abbr>Ph</abbr></line>
                <line xml:id="lad" rend="indent-10"><mod rend="strikethrough" hand="#g_bl">Doch kaum</mod>
                    <f:overw>
                        <f:under>b</f:under>
                        <f:over>B</f:over>
                    </f:overw>efreyt durch Caſtor deine<f:overw>
                        <f:under>s</f:under>
                        <f:over>n</f:over>
                    </f:overw></line>
                <line xml:id="lae" rend="indent-40">Bruders, <mod rend="strikethrough" hand="#g_bl">Kraft</mod> ſtandeſt</line>
                <line xml:id="laf" rend="indent-60"><app>
                        <lem>du</lem>
                        <rdg resp="wa-app"/>
                    </app></line>
                <line xml:id="lag" rend="indent-10">Umworben gleich von auserleſner Helden</line>
                <line xml:id="lah" rend="indent-70">Chor Schaar</line>
                <line xml:id="lai" rend="centered"><abbr>H.</abbr></line>
                <line xml:id="laj" rend="indent-20">Doch meine Gunſt wie ich es geſtehen wil</line>
                <line f:left="#eignes" xml:id="lak" type="inter">reines</line>
                <line xml:id="lal" rend="indent-20">Gewann Patroklus <anchor xml:id="eignes"/><mod rend="strikethrough" hand="#g_bl">eignes</mod> Ebenbild</line>
                <line xml:id="lam" rend="indent-60">Achills</line>
                <space dim="vertical" quantity="2" unit="lines"/>
                <anchor xml:id="Achilles"/>
            </zone>
            <zone f:right-left="#Streiche" f:top="#Streiche">
                <line xml:id="lar"><handShift new="#ri_bl_lat"/><space dim="horizontal" quantity="3" unit="chars"/>⸓</line>
            </zone>
            <zone f:right-left="#Jannis" f:top="#Jannis">
                <line xml:id="laq"><handShift new="#ri_bl_lat"/><f:grLine f:orient="vert" hand="#ri_bl_lat" f:style="linear"/>v<space dim="horizontal" quantity="3" unit="chars"/></line>
            </zone>
            <zone f:top="#Jannis-aber">
                <line><handShift new="#g_tr_lat"/><f:grLine f:style="linear" f:orient="horiz" hand="#g_tr_lat"/><space dim="horizontal" quantity="4" unit="chars"/></line>
            </zone>
            <zone f:right-left="#lai" f:bottom="#lai">
                <line><handShift new="#g_bl"/><f:grLine f:orient="horiz" f:style="linear" hand="#g_bl"/></line>
            </zone>
            <metamark function="used" hand="#g_bl" spanTo="#Glied"/>
            <zone f:bottom="#Hauptzone" f:left="#Hauptzone" rotate="270" xml:id="Randzone">
                <line xml:id="lan"><handShift new="#g_bl"/>Wie Droßlen ſ<orig>a</orig>mtlich</line>
                <line xml:id="lao" rend="indent-10">aufgeh<orig>a</orig>ngt in Reih und</line>
                <line xml:id="lap" rend="indent-10">Glied</line>
                <anchor xml:id="Glied"/>
            </zone>
        </surface>
    </sourceDoc>
