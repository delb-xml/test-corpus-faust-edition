        <surface>
            <zone type="main" xml:id="mainzone">
                <line xml:id="la" rend="centered">
                    <handShift new="#jo_t"/>
                    <abbr><hi rend="underline"> Phork</hi>.</abbr>
                </line>
                <line xml:id="lb"><anchor xml:id="Habt"/><hi hand="#ri_bl" rend="underline">Habt</hi><c xml:space="preserve"> </c><hi hand="#ri_bl" rend="underline">ihr</hi> Geduld des Vortrags langgedehnten Zug,</line>
                <line f:left="#giebts" xml:id="lc" type="inter"><handShift new="#g_t"/>ſind’s.</line>
                <line xml:id="ld"><handShift new="#jo_t"/>Still anzuhören? Mancherley Geſchichten
                        <anchor xml:id="giebts"/><mod rend="strikethrough" hand="#g_t">giebt’s</mod></line>
                <line f:left="#giebts" xml:id="le" type="inter"><handShift new="#ri_bl"/>ſind’s</line>
                <line xml:id="lf" rend="centered">
                    <handShift new="#jo_t"/>
                    <hi rend="underline">Chor.</hi>
                </line>
                <line xml:id="lg">Geduld genug! Zuhörend leben wir indeß.</line>
                <line xml:id="lh" rend="centered">
                    <abbr><hi rend="underline">Phork</hi>.</abbr>
                </line>
                <line f:left="#seinen" xml:id="li" type="inter"><handShift new="#ri_bl"/>edlen</line>
                <line xml:id="lj"><handShift new="#jo_t"/>Dem der zu Hauſe verharrend <anchor xml:id="seinen"/><mod rend="strikethrough" hand="#ri_bl">ſeinen</mod> Schatz bewahrt,</line>
                <line xml:id="lk">Und hoher Wohnung Mauern auszukitten weiß,</line>
                <line xml:id="ll" type="inter">
                    <retrace hand="#g_t"><handShift new="#ri_bl"/><f:ins f:at="#das" f:orient="right" hand="#g_t"><f:ins f:at="#das" f:orient="right" hand="#ri_bl">auch</f:ins></f:ins></retrace>
                </line>
                <line xml:id="lm"><handShift new="#jo_t"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl"><handShift new="#ri_bl"/>⌞<handShift new="#jo_t"/>So<handShift new="#ri_bl"/>⌟ <handShift new="#jo_t"/></mod></mod><c xml:space="preserve"> </c><f:overw>
                        <f:under>w</f:under>
                        <f:over><handShift new="#g_t"/>W</f:over>
                    </f:overw><handShift new="#jo_t"/>ie <anchor xml:id="das"/>das Dach zu ſichern
                    vor des Regens Drang,</line>
                <line xml:id="ln">Dem wird es wohlgeh<mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">e</mod></mod>n lange Lebenstage durch;</line>
                <line xml:id="lo" type="inter">
                    <retrace hand="#g_t"><handShift new="#ri_bl"/><f:ins f:at="#Richtschnur" f:orient="right">e</f:ins></retrace>
                </line>
                <line xml:id="lp"><anchor xml:id="Wer"/><handShift new="#jo_t"/>Wer aber ſeiner
                    Schwelle heilige Richt<anchor xml:id="Richtschnur"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">ſchnur</mod></mod> leicht</line>
                <line f:left="#frevelhaft" xml:id="lq" type="inter">
                    <retrace hand="#g_t"><handShift new="#ri_bl"/>freventlich</retrace>
                </line>
                <line xml:id="lr"><handShift new="#jo_t"/>Mit flüchtigen Solen überſchreitet
                        <anchor xml:id="frevelhaft"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">frevelhaft</mod></mod>,</line>
                <line xml:id="ls">Der findet, wiederkehrend, wohl den alten Platz,</line>
                <line xml:id="lt">Doch umgeändert alles, wo nicht gar zerſtört.</line>
                <line xml:id="lu" rend="centered"><hi rend="underline">Helena</hi>.</line>
                <line xml:id="lv">Wozu dergleichen wohlbekannte Sprüche hier!</line>
                <line xml:id="lw" type="inter">
                    <retrace hand="#g_t"><handShift new="#ri_bl"/><f:ins f:at="#Verdrießliches" f:orient="right" hand="#g_t"><f:ins f:at="#Verdrießliches" f:orient="right" hand="#ri_bl">an</f:ins></f:ins></retrace>
                </line>
                <line xml:id="lx"><anchor xml:id="Du"/><handShift new="#jo_t"/>Du willſt
                    erzählen, rege nicht <anchor xml:id="Verdrießliches"/>Verdrießliches<handShift new="#g_t"/>. <handShift new="#jo_t"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">auf.</mod></mod></line>
                <line xml:id="ly" rend="centered"><abbr><hi rend="underline">Phork</hi>.</abbr></line>
                <line xml:id="lz" type="inter">
                    <seg f:left="#Es">
                        <retrace hand="#g_t"><handShift new="#ri_bl"/>3</retrace>
                    </seg>
                    <seg f:left="#ist">
                        <retrace hand="#g_t"><handShift new="#ri_bl"/>2</retrace>
                    </seg>
                    <seg f:left="#geschichtlich">
                        <retrace hand="#g_t"><handShift new="#ri_bl"/>1</retrace>
                    </seg>
                    
                        <retrace hand="#g_t"><f:ins f:at="#ein" f:orient="left" hand="#ri_bl">
                                <handShift new="#ri_bl"/>iſt
                        </f:ins></retrace>
                    
                    <seg f:left="#Vorwurf">
                        <retrace hand="#g_t"><handShift new="#ri_bl"/>keineswegs.</retrace>
                    </seg>
                </line>
                <line xml:id="laa"><handShift new="#jo_t"/><anchor xml:id="Es"/>Es iſ<anchor xml:id="ist"/>t <f:overw>
                        <f:under>g</f:under>
                        <f:over><handShift new="#g_t"/>G</f:over>
                    </f:overw><handShift new="#jo_t"/>eſchi<anchor xml:id="geschichtlich"/>chtlich,
                        <mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">keineswegs</mod></mod>
                    <anchor xml:id="ein"/>ein Vor<anchor xml:id="Vorwurf"/>wurf <mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">iſt’s</mod></mod>.</line>
                <line f:left="#Seeräuberisch" xml:id="lab" type="inter">
                    <retrace hand="#g_t"><handShift new="#ri_bl"/>Raubſchiffend
                        ruderte</retrace>
                </line>
                <line xml:id="lac"><anchor xml:id="Seeräuberisch"/><handShift new="#jo_t"/><mod rend="strikethrough" hand="#ri_bl"><mod rend="strikethrough" hand="#g_t">Seeräuberiſch</mod><c xml:space="preserve"> </c><mod rend="strikethrough" hand="#g_t">umſchiffte</mod></mod> Menelas, von Bucht zu Bucht,</line>
                <line f:left="#freundlich" xml:id="lad" type="inter"><handShift new="#g_t"/>feindlich</line>
                <line xml:id="lae"><handShift new="#jo_t"/>Geſtad und Inſeln, alles ſtreift er
                        <anchor xml:id="freundlich"/><mod rend="strikethrough" hand="#g_t">freundlich</mod>
                    an,</line>
                <line xml:id="laf"><handShift new="#jo_t"/>Mit Beute wiederkehrend, wie ſie
                    drinnen ſtarrt.</line>
