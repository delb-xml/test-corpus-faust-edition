    <sourceDoc>
        <surface>
            <zone type="main" xml:id="mainzone">
                <line rend="right" xml:id="la"><handShift new="#g_bl"/><fw type="folioNum">20</fw></line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lb"><handShift new="#jo_t"/>Vielleicht auch irrt ſie zweifelhaft im
                    Labyrinth</line>
                <line f:left="#zerstueckelt" type="inter" xml:id="lc"><handShift new="#g_bl"/>aus
                    vielen</line>
                <line xml:id="ld"><handShift new="#jo_t"/>Der wunderſam <anchor xml:id="zerstueckelt"/><hi hand="#g_bl" rend="underline">zerſtückelt</hi>-einsgewordnen Burg</line>
                <line type="inter" xml:id="le"><retrace hand="#g_t"><f:ins f:at="#Begruessung" f:orient="left" hand="#ri_bl"><handShift new="#ri_bl"/>Hochbe</f:ins></retrace></line>
                <line xml:id="lf"><handShift new="#jo_t"/>Den Herrn erfragend fürſtliche<hi hand="#ri_bl" rend="underline">r <anchor xml:id="Begruessung"/><mod rend="strikethrough" hand="#g_t">Be</mod></hi>grüßung halb.</line>
                <line f:left="#schon" type="inter" xml:id="lg"><retrace hand="#g_t"><handShift new="#ri_bl"/>ſich</retrace></line>
                <line xml:id="lh"><handShift new="#jo_t"/>Doch ſieh dort oben regt <mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">ſich</mod></mod> in Menge <anchor xml:id="schon"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">ſchon</mod></mod>
                    <retrace hand="#g_t"><handShift new="#ri_bl"/>allbereits,</retrace></line>
                <line xml:id="li"><handShift new="#jo_t"/>In Gallerien, am Fenſter, in Portalen
                    raſch</line>
                <line xml:id="lj">Sich hin und her bewegend viele Dienerſchaft;</line>
                <line f:left="#Empfang" type="inter" xml:id="lab"><handShift new="#ri_bl"/>Gaſtem</line>
                <line xml:id="lk"><handShift new="#jo_t"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">Das</mod></mod> <mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">kündet</mod></mod> <mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">an</mod></mod> <mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">vornehm</mod></mod><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">-willkom̄<handShift new="#ri_bl"/><unclear cert="high">’</unclear><handShift new="#jo_t"/><mod rend="strikethrough" hand="#ri_bl">e</mod>nen</mod></mod> <anchor xml:id="Empfang"/><mod rend="strikethrough" hand="#g_t"><mod rend="strikethrough" hand="#ri_bl">Empfang.</mod></mod></line>
                <line type="inter" xml:id="ll"><retrace hand="#g_t"><handShift new="#ri_bl"/>Vornehm-willkommnen Gaſtempfang verkündet es.</retrace></line>
                <line rend="centered" xml:id="lm"><handShift new="#jo_t"/><hi rend="underline">Chor</hi>.</line>
                <line type="inter" xml:id="ln"><handShift new="#ri_bl"/><seg f:left="#Das">4</seg>
                    <seg f:left="#Herz">5</seg>
                    <seg f:left="#geht">2</seg>
                    <seg f:left="#mir">3</seg>
                    <seg f:left="#auf">1</seg></line>
                <line rend="indent-20" xml:id="Das-dahin"><handShift new="#jo_t"/>D<anchor xml:id="Das"/>as He<anchor xml:id="Herz"/>rz ge<anchor xml:id="geht"/>ht
                        m<anchor xml:id="mir"/>ir a<anchor xml:id="auf"/>uf! o! ſeht nur
                    dahin,</line>
                <line rend="indent-20" xml:id="lo">Wie ſittig herab verweilenden Tritts</line>
                <line rend="indent-20" xml:id="lp">Die holdeſte Schaar anſtändig bewegt</line>
                <line rend="indent-20" xml:id="lq">Geregelten Zug. Auf weſſen Befehl</line>
                <line rend="indent-20" xml:id="lr">Erſcheinen, gereiht, gebildet ſo
                    früh,</line>
                <line rend="indent-20" xml:id="ls">Von Jünglingsknaben das herrliche
                    Volk?</line>
                <line rend="indent-20" xml:id="lt">Was wundert mich mehr? ein zierlicher
                    Gang,</line>
                <line rend="indent-20" xml:id="lu">Das lockige Haupt, die blendende
                    Stirn,</line>
                <line rend="indent-20" xml:id="lv">Der Wängelein Paar wie Pfirſiche
                    roth,</line>
                <line rend="indent-20" xml:id="lw">Und eben auch ſo mit Wolle beflaumt.</line>
                <line rend="indent-20" xml:id="lx">Gern biß ich hinein, doch ſchaud<hi hand="#ri_bl" rend="underline">r’</hi> ich davor;</line>
                <line rend="indent-20" xml:id="ly">In ähnlichem Fall erfüllte der Mund</line>
                <line rend="indent-20" xml:id="lz">Sich, gräßlich zu ſagen, mit Aſche.</line>
            </zone>
            <zone f:right-left="#lf" f:top="#lf">
                <line xml:id="lac"><handShift new="#ri_bl"/>⸓<space dim="horizontal" quantity="1" unit="chars"/></line>
            </zone>
            <zone f:right-left="#Das-dahin" f:top="#Das-dahin">
                <line xml:id="laa"><handShift new="#g_bl"/><num>V.</num><gap quantity="8" unit="chars"/></line>
            </zone>
            <surface type="patch">
                <zone f:left="#mainzone" f:top-bottom="#lm">
                    <line rend="right" xml:id="lap"><handShift new="#xx_bl_lat"/><fw type="folioNum">20<hi rend="sup">a</hi></fw></line>
                    <line rend="centered" xml:id="Anapaeste"><handShift new="#ri_t_lat"/><mod rend="strikethrough" hand="#g_bl"><hi rend="underline">Anapäſte</hi></mod></line>
                    <line xml:id="lbp">Aufgeht mir das Herz! o, Seht nur dahin,</line>
                    <line xml:id="lcp">Wie so sittig herab mit verweilendem Tritt</line>
                    <line xml:id="ldp">Jungholdeſte Schaar anſtändig bewegt</line>
                    <line xml:id="lep">Den geregleten Zug<handShift new="#ri_bl_lat"/>.
                            <handShift new="#ri_t_lat"/>wie auf weſſen Befehl</line>
                    <line xml:id="lfp">Nur erſcheinen gereiht und gebildet so früh</line>
                    <line type="inter" xml:id="lgp"><handShift new="#ri_bl_lat"/><seg f:left="#Aus-Volk">Von</seg><seg f:left="#das">ein</seg><seg f:left="#herrliche">s</seg></line>
                    <line xml:id="Aus-Volk"><handShift new="#ri_t_lat"/><mod rend="strikethrough" hand="#ri_bl">Aus</mod> Jünglingsknaben <anchor xml:id="das"/>das herrliche<anchor xml:id="herrliche"/> Volk?</line>
                    <line xml:id="lhp">Was bewundr’ ich zu<handShift new="#ri_bl_lat"/>‿<handShift new="#ri_t_lat"/>meiſt! Iſt es zierlicher
                        Gang,</line>
                    <line xml:id="lip">Oder des Haupts Lockhaar, um die blendende Stirn</line>
                    <line xml:id="ljp">Oder der Wänglein Paar, wie die Pfirsiche roth</line>
                    <line xml:id="lkp">Und eben auch so weichwollig beflaumt</line>
                    <line xml:id="llp">Gern biſs ich hinein, doch ich schaudre davor</line>
                    <line xml:id="lmp">Denn in ähnlichem Fall, da erfüllte der Mund</line>
                    <line xml:id="lnp">Sich gräſslich zu sagen, mit Aſche.</line>
                </zone>
                <zone>
                    <line f:left="#lbp" f:top="#Anapaeste" xml:id="lop"><handShift new="#g_bl"/><num>V.</num></line>
                </zone>
                <zone f:right-left="#Aus-Volk" f:top="#Aus-Volk">
                    <line xml:id="lpp"><handShift new="#ri_bl"/>⸓</line>
                </zone>
            </surface>
