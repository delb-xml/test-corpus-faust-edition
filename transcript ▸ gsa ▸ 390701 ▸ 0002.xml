    <sourceDoc>
        <surface>
            <zone rotate="270" type="main" xml:id="mainzone"><!-- Hauptzone, quer -->
                <metamark function="used" hand="#g_bl" spanTo="#Augen-sehn"/>
                <line xml:id="la"><handShift new="#g_bl"/>Er konnt <app>
                        <lem>uns</lem>
                        <rdg resp="wa-app"><unclear cert="high">un</unclear>s</rdg>
                        <rdg resp="wa-app"><unclear cert="low">e</unclear>s</rdg>
                    </app> alles nehmen doch begnügt er ſich</line>
                <line f:left="#und-also" type="inter" xml:id="lb">hies es nicht Tribut</line>
                <line xml:id="lc">Mit wenigem Geſchenck<anchor xml:id="Geschenck"/><c xml:space="preserve"> </c><anchor xml:id="und-also"/><mod rend="strikethrough" hand="#g_bl">und <app>
                            <lem>alſo</lem>
                            <rdg resp="wa-app">ſo</rdg>
                        </app> ſchützt er mich</mod></line>
                <line f:left="#Geschenck" xml:id="lai" type="inter">en</line>
                <line xml:id="ld">Es iſt ein muntrer keker wohlgebildeter</line>
                <line xml:id="le">Verſt<orig>a</orig>ndiger Mann wie unter Griechen wenige ſind</line>
                <line xml:id="lf">Man nennt ſich zwar Barbaren, doch ich
                        d<orig>a</orig>chte nich<supplied>t</supplied></line>
                <line type="inter" xml:id="lg"><f:ins f:at="#vor" f:orient="right" hand="#g_bl">wie</f:ins></line>
                <line xml:id="lh">Daß einer grauſam wäre <anchor xml:id="vor"/>vor
                    Ilium</line>
                <line xml:id="li">Gar mancher Held ſich menſchenfreſſeriſch bewies.</line>
                <line xml:id="lj">Ich acht auf ſeine <app>
                        <lem>Grosmuth</lem>
                        <rdg resp="wa-app">Großmuth</rdg>
                    </app> ihm vertraut ich mich</line>
                <line xml:id="lk">Und ſeine Burg die ſolltet ihr mit Augen ſehn</line>
                <anchor xml:id="Augen-sehn"/>
                <line f:left="#das" type="inter" xml:id="ll">plumpe</line>
                <line xml:id="lm">Das iſt was anders als das<anchor xml:id="das"/>
                    Mauerwerck</line>
                <line f:left="#gap" type="inter" xml:id="ln">nicht<supplied>s</supplied></line>
                <line xml:id="laj" type="inter"><seg f:left="#gap">nichts</seg><seg f:left="#aufgethuermt"><handShift new="#g_t"/>w<orig>a</orig>lzt</seg></line>
                <line f:pos="between" xml:id="lak"><handShift new="#g_bl"/>Das eure Väter mir
                    nichts dir <anchor xml:id="gap"/><mod rend="strikethrough" hand="#g_bl"><gap precision="medium" quantity="5" unit="chars"/></mod> aufgebaut</line>
                <line f:pos="over" xml:id="lo"><handShift new="#g_t"/>Das eure Väter mir nichts
                    dir nichts aufge<anchor xml:id="aufgethuermt"/><mod rend="strikethrough" hand="#g_t">thürmt</mod></line>
                <line f:pos="between" xml:id="lal" type="inter"><handShift new="#g_bl"/>Cyclopiſch wie Cyclopen rohen Stein ſogleich gefügt</line>
                <line f:pos="over" type="inter" xml:id="lp"><handShift new="#g_t"/>Cyclopisch wie
                    Cyclopen, rohen Stein ſogleich</line>
                <line f:pos="between" xml:id="lq"><handShift new="#g_bl"/><f:overw>
                        <f:under>Den</f:under>
                        <f:over>Auf</f:over>
                    </f:overw> rohen Stein zum rohen Steine fügend</line>
                <line f:pos="over" xml:id="lam"><handShift new="#g_t"/>Dem rohen Steine fügend
                        <handShift new="#g_bl"/>plump und</line>
                <line type="inter" xml:id="lr"><f:ins f:at="#und" f:orient="right" hand="#g_bl">wage</f:ins></line>
                <line xml:id="ls">Doch alles grad und<anchor xml:id="und"/> recht und
                    regelhafft</line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <metamark function="used" hand="#g_bl" spanTo="#Fensterchen"/>
                <line xml:id="lt">Der Hof geräumig</line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lu">Das alles hatten ſie geziemlich auferbaut</line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <line xml:id="lv">Sie hatten Zeit es <choice>
                        <sic>ſich</sic>
                        <corr resp="wa-app">ſind</corr>
                    </choice> nur volle zwanzig Jahr</line>
                <line xml:id="lw">Und Gallerien, S<orig>a</orig>ulchen Kn<orig>a</orig>uf<unclear cert="high">ch</unclear>en, Fenſterchen</line>
                <line xml:id="lx">Und wieder S<orig>a</orig>ulchen Kn<orig>a</orig>ufchen wieder
                    Fenſterchen</line>
                <anchor xml:id="Fensterchen"/>
            </zone>
            <zone f:left-right="#mainzone" f:top="#mainzone" rotate="180"><!-- rechts am Rand, urspr <zone f:bottom-top="#mainzone" f:left="#mainzone" rotate="180"> -->
                <metamark function="used" hand="#g_bl" spanTo="#tanzen"/>
                <line xml:id="ly"><handShift new="#g_bl"/><f:overw>
                        <f:under>D</f:under>
                        <f:over>G</f:over>
                    </f:overw>anz allerliebſt doch Saale wie die Welt ſo weit</line>
                <line xml:id="lz">Da k<orig>o</orig>nnt ihr tanzen.</line>
                <anchor xml:id="tanzen"/>
                <metamark function="used" spanTo="#nicht-so"/>
                <line rend="centered" xml:id="laa"><choice>
                        <abbr>Ch.</abbr>
                        <expan>Ch<unclear cert="unknown">or</unclear></expan>
                        <expan>Ch<unclear cert="unknown">orführerin</unclear></expan>
                    </choice></line>
                <line rend="indent-50" xml:id="lab">Giebt es T<orig>a</orig>nzer auch</line>
                <line rend="indent-70" xml:id="lac">daſelbſt</line>
                <line rend="centered">Ph</line>
                <line xml:id="lad"> Und welche! <anchor xml:id="Goldenlockige"/><app>
                        <lem>Goldenlockige</lem>
                        <rdg resp="wa-app">Goldlockige</rdg>
                    </app> und frische Buben</line>
                <line f:left="#Goldenlockige" xml:id="lae"><mod rend="strikethrough" hand="#g_bl">duftende nach
                        Knabenart</mod></line>
                <line xml:id="laf">Die duften Jugend Paris duftete nicht ſo</line>
                <anchor xml:id="nicht-so"/>
            </zone>
            <zone f:right-left="#mainzone" f:top="#mainzone"><!-- urspr <zone f:top-bottom="#mainzone" f:left="#mainzone"> -->
                <!-- links am Rand -->
                <metamark function="used" hand="#g_bl" spanTo="#Lästermaul"/>
                <line xml:id="lag"><handShift new="#g_bl"/>Als er der Königin zu nah k<space dim="horizontal" precision="medium" quantity="3" unit="chars"/></line>
                <line rend="indent-70" xml:id="lah">Läſtermaul</line>
                <anchor xml:id="Lästermaul"/>
            </zone>
