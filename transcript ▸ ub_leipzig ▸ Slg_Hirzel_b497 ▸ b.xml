                <metamark function="used" hand="#g_ro" spanTo="#anchor_2"/>
                <line xml:id="lbc"><handShift new="#g_t"/>Bis zur Wurzel glühn die hohlen</line>
                <line xml:id="lbd">St<orig>a</orig>mme, dunckelroth im Glühn.</line>
                <line xml:id="lbe">Was dem Blick ſich ſonſt empfohlen</line>
                <line xml:id="lbf"><mod rend="strikethrough" hand="#g_t">Und</mod> Jahrhunderte<seg type="inbetween">n</seg><c xml:space="preserve"> </c><mod rend="strikethrough" hand="#g_t">ſind hin</mod> iſt hin</line>
