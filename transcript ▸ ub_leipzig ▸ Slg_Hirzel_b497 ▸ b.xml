        <surface><!-- TODO Hs nochmal im Original lesen -->
            <zone type="main" xml:id="mainzone">
                <line xml:id="la"><handShift new="#xx_bl"/><note>309</note><!-- C(1)41, S. 309 --></line>
                <space dim="vertical" quantity="0.5" unit="lines"/>
                <metamark function="used" hand="#g_ro" spanTo="#anchor_1"/>
                <line f:left="#nicht" type="inter" xml:id="lb"><handShift new="#g_bl"/>ſeh ich <mod rend="strikethrough" hand="#g_bl">blitzen</mod> ſprühen</line>
                <line xml:id="lc">Feuer blicke, <anchor xml:id="nicht"/><mod rend="strikethrough" hand="#g_bl">nicht <app>
                            <lem>ſeh</lem>
                            <rdg resp="#wa-app">ſah</rdg>
                        </app> ichs gerne</mod></line>
                <line f:left="#durch" type="inter" xml:id="ld">der Linden <app>
                        <lem>Dopel<unclear cert="high">nacht</unclear></lem>
                        <rdg resp="#wa-app">dichte</rdg>
                    </app></line>
                <line xml:id="le">Durch<anchor xml:id="durch"/>
                    <mod rend="strikethrough" hand="#g_bl"><app>
                            <lem>die ſchweigend ſtille Luft</lem>
                            <rdg resp="#wa-app"><gap atLeast="1" unit="words"/></rdg>
                        </app></mod></line>
                <line f:left="#ist_es" type="inter" xml:id="lf">Immer ſt<orig>a</orig>rcker</line>
                <line xml:id="lg"><mod rend="strikethrough" hand="#g_bl">Droben iſt <anchor xml:id="ist_es"/>es</mod><c xml:space="preserve"> </c><f:overw>
                            <f:under>Unten</f:under>
                            <f:over>Immer</f:over>
                        </f:overw> ſt<orig>a</orig>rcker ſcheint<supplied evidence="internal">s</supplied> zu</line>
                <line rend="indent-70" xml:id="lh">gl<orig>u</orig>hen</line>
                <line xml:id="li">Von der Zugluft angefacht</line>
                <line xml:id="lj">Ach die alte Hütte <f:overw>
                        <f:under><unclear cert="high" resp="#wa-app">brennet</unclear></f:under>
                        <f:over><mod rend="strikethrough" hand="#g_bl">flackert</mod></f:over>
                    </f:overw> lodert</line>
                <line f:left="#so_lang" type="inter" xml:id="lk">ſo</line>
                <line xml:id="ll">Die <anchor xml:id="so_lang"/> lang’ bemoost geſtanden</line>
                <line xml:id="lm">Schnelle Hülfe <f:overw>
                        <f:under>iſt</f:under>
                        <f:over>wird</f:over>
                    </f:overw> gefodert</line>
                <line xml:id="ln">Keine Rettung iſt zu Handen</line>
                <line xml:id="lo">Ach die guten alten L<retrace>eu</retrace>te</line>
                <line xml:id="lp">Sonſt ſo ſorglich mit dem Feuer</line>
                <line f:left="#zur_Beute" type="inter" xml:id="lq">der Flammen</line>
                <line xml:id="lr">Werden ſie <anchor xml:id="zur_Beute"/><mod rend="strikethrough" hand="#g_bl">zur</mod> Be<retrace>ute</retrace></line>
                <line f:left="#frechen" type="inter" xml:id="ls">traurig</line>
                <line xml:id="Einem"><f:overw>
                        <f:under>E</f:under>
                        <f:over>e</f:over>
                    </f:overw>inem <anchor xml:id="frechen"/><mod rend="strikethrough" hand="#g_bl">frechen</mod> Abent<retrace>euer</retrace></line>
                <line type="inter" xml:id="lt">
                    <f:ins f:at="#es_wie" f:orient="right">roth in</f:ins>
                </line>
                <line xml:id="lu">Fl<retrace>am̄</retrace>e<retrace>nd</retrace> flammt es <anchor xml:id="es_wie"/><app>
                        <lem>und die</lem>
                        <rdg resp="#wa-app"><mod rend="strikethrough" hand="#g_bl">wie</mod> in</rdg>
                    </app> Gluten</line>
                <line type="inter" xml:id="lv"><f:ins f:at="#Moosgestelle" f:orient="left">ſchwarze</f:ins></line>
                <line xml:id="lw">Steht das <mod rend="strikethrough" hand="#g_bl"><app>
                            <lem resp="#wa-app">alte</lem>
                            <rdg resp="#wa-app"><unclear cert="low">arme</unclear></rdg>
                        </app></mod><anchor xml:id="Moosgestelle"/> Moosgeſtelle.</line>
                <line xml:id="lx">Retteten ſich nur die Guten</line>
                <line type="inter" xml:id="ly">Aus der ſchnell entbrannten H<orig>o</orig>lle</line>
                <line xml:id="lz">
                    <mod rend="strikethrough" hand="#g_bl"><app><lem resp="#wa-app">Muß</lem></app><unclear cert="low">ten ſich aus dem</unclear><c xml:space="preserve"> </c><app>
                            <lem><unclear cert="low">Hauſe fliehen</unclear></lem>
                            <rdg resp="#wa-app">Geſchick <gap atLeast="1" unit="words"/></rdg>
                        </app>
                    </mod>
                </line><!-- Moering: Müsten sich aus den Schlaf ...e / Henzel: Müßen sie dem Hause fliehen -->
                <line f:left="#steigt" type="inter" xml:id="laa"><mod rend="strikethrough" hand="#g_bl"><gap precision="medium" quantity="6" unit="chars"/></mod>
                    <gap precision="medium" quantity="4" unit="chars"/>
                    <gap precision="medium" quantity="5" unit="chars"/></line>
                <line xml:id="lab"><mod rend="strikethrough" hand="#g_bl"><app>
                            <lem><unclear cert="high"><f:overw>
                                        <f:under>Greul</f:under>
                                        <f:over>Z<orig>u</orig>ng</f:over>
                                    </f:overw>ich ſt<anchor xml:id="steigt"/><gap precision="medium" quantity="5" unit="chars"/></unclear>
                                <gap precision="medium" quantity="26" unit="chars"/>
                            </lem>
                            <rdg resp="#wa-app"><unclear>Greulich <app>
                                        <lem>ſteigt</lem>
                                        <rdg>ſtürzt</rdg>
                                    </app></unclear></rdg>
                        </app>
                        <gap precision="medium" quantity="9" unit="chars"/>
                        <app>
                            <lem><gap precision="medium" quantity="14" unit="chars"/></lem>
                            <rdg resp="#wa-app">ſtetig ſteigen</rdg>
                        </app></mod></line>
                <line xml:id="lac">Zwiſchen Bl<orig>a</orig>ttern zwiſchen Zweigen</line>
                <line f:left="#Alten" type="inter" xml:id="lad">D<unclear cert="high">ürren</unclear></line>
                <line xml:id="lae">Wie die <anchor xml:id="Alten"/><mod rend="strikethrough" hand="#g_bl">Alten</mod> <orig>Ae</orig>ſte b<retrace>r</retrace>ennen</line>
                <line xml:id="laf">Und ſie glühn und ſtürzen ein</line>
                <line type="inter" xml:id="lag">Sollt ihr Augen dies erkennen!</line>
                <line xml:id="lah"><mod rend="strikethrough" hand="#g_bl">Ach <app>
                    <lem><unclear cert="high">hier</unclear></lem>
                            <rdg resp="#wa-app">was</rdg>
                        </app> muß ich <app>
                            <lem>dies</lem>
                            <rdg resp="#wa-app">es</rdg>
                        </app> erblicken</mod></line>
                <line xml:id="lai">Muß ich ſo weitſichtig ſey<supplied evidence="internal">n</supplied>?</line>
                <line xml:id="laj">Das Kapellchen bricht zuſammen</line>
                <line type="inter" xml:id="lak"><f:ins f:at="#anchor_4" f:orient="right">S<unclear cert="high">turz</unclear></f:ins></line>
                <line xml:id="lal">Von de<f:overw xml:space="default">
                        <f:under>r</f:under>
                        <f:over>m</f:over>
                    </f:overw><anchor xml:id="anchor_4"/> <app xml:space="default">
                        <lem>Sch<unclear cert="high"><f:overw>
                            <f:under>l<orig>a</orig>g</f:under>
                            <f:over>ich</f:over>
                        </f:overw>ten</unclear></lem>
                        <rdg resp="#wa-app" type="substantial">glühnden</rdg>
                    </app> Laſt<space dim="horizontal" quantity="5" unit="chars"/><orig>Ae</orig>ſte Sturz und Laſt</line>
                <line type="inter" xml:id="lam">Flamme hat mit ſpitzen</line>
                <line xml:id="lan"><mod rend="strikethrough" hand="#g_bl">Und</mod><c xml:space="preserve"> </c><mod rend="strikethrough" hand="#g_bl">ſo haben</mod><c xml:space="preserve"> </c><mod rend="strikethrough" hand="#g_bl">Flamm nach</mod> Flammen</line>
                <line xml:id="lao">S<f:overw><f:under><unclear cert="high">ich</unclear></f:under><f:over>chon</f:over></f:overw> die Gipfel angefaſt</line>
                <line xml:id="lap"><f:overw xml:space="default">
                        <f:under>Und</f:under>
                        <f:over>Ja</f:over>
                    </f:overw> <mod rend="strikethrough" hand="#g_bl">ſchon</mod> gl<orig>u</orig>hn die h<retrace>o</retrace><seg rend="inbetween">h</seg>len St<orig>a</orig>mme</line>
                <line xml:id="laq">Funcken ſteigen Funcken ſpr<orig>u</orig>hn</line>
                <line xml:id="lar">Und Jahrhunderte ſind <mod rend="strikethrough" hand="#g_bl">Tod</mod> hin.</line>
                <line rend="centered" xml:id="las">
                    <f:grLine f:orient="horiz" f:style="linear" hand="#g_bl"/>
                </line>
                <line xml:id="lat">Von oben <f:overw>
                        <f:under><unclear cert="high">–</unclear>
                            <gap quantity="1" unit="chars"/></f:under>
                        <f:over>w</f:over>
                    </f:overw>elch ein ſingend wimmern</line>
                <line xml:id="lau">Hier iſt ein Wort <app>
                        <lem><unclear cert="high">–</unclear></lem>
                        <rdg resp="#wa-app">ein</rdg>
                    </app> Ton zu ſpat</line>
                <line xml:id="lav">Der <app>
                        <lem>Th<orig>u</orig>r<unclear cert="high">n</unclear>er</lem>
                        <rdg resp="#wa-app">Thürmer</rdg>
                    </app> Jammert, mich im Innern</line>
                <line xml:id="law">Verdrieß<supplied evidence="internal">t</supplied> die ungeduldige That<anchor xml:id="anchor_1"/></line>
            </zone>
            <zone f:bottom="#Einem" f:right-left="#mainzone">
