    <text>
        <body>
            <pb n="03"/>
            <lg type="stanza">
                <head>Strophe</head>
                <l n="9629">Nennſt Du ein Wunder dieſs,</l>
                <l n="9630">Cretas Erzeugte?</l>
                <l n="9631">Dichtend Belehrenden</l>
                <l n="9632"><seg f:questionedBy="#ri">Haſt <del><add>denn</add></del> <seg xml:id="niemals">niemals</seg> <seg xml:id="Du">Du</seg> zugelauſcht?</seg></l>
                <l n="9633">Niemals noch gehört Ioniens</l>
                <l n="9634">Nie vernommen auch Hellas</l>
                <l n="9635">Urväterlicher Sagen</l>
                <l n="9636">Göttlich-heldenhaften Reichthum?</l>
            </lg>
            <lg type="stanza">
                <head>
                    <abbr><hi rend="underline">Antiſtr</hi>.</abbr>
                </head>
                <l n="9637">Alles was je geſchieht</l>
                <l n="9638">Unserer Tage</l>
                <l n="9639">Trauriger Nachklang iſt’s</l>
                <l n="9640">
                    <seg f:questionedBy="#ri">Der Tage herrlicher Ahnherrn</seg>
                </l>
                <l n="9641">Nicht vergleicht sich Dein Erzählen</l>
                <l n="9642">Dem was liebliche Lüge</l>
                <l n="9643">Glaubhaftiger als Wahrheit</l>
                <l n="9644">Von dem Sohne sang der Maja.</l>
            </lg>
            <pb n="01"/>
            <lg type="stanza">
                <head>2. Strophe</head>
                <l n="9645">Dieſen zierlich und kräftig <subst f:proposedBy="#ri">
                        <del>auch</del>
                        <add>doch</add>
                    </subst></l>
                <l n="9646">Kaum geborenen Säugling</l>
                <l n="9646">Faltet in <subst>
                        <del>reinſte <subst>
                                <del><restore>Windeln</restore></del>
                                <add><del f:revType="instant">Woll</del></add>
                            </subst></del>
                        <add>reinſter Windeln <subst>
                                <del><subst>
                                    <del><gap unit="chars" quantity="3" precision="medium"/>z</del>
                                        <add>Vlieſs</add>
                                    </subst></del>
                                <add f:proposedBy="#ri" f:rejectedBy="#ri">Blume</add>
                                <add>Flaum</add>
                            </subst></add>
                    </subst></l>
                <l n="9648">Strenget in köſtlicher Wickeln <subst>
                        <del><subst>
                                <del>Pracht</del>
                                <add>Band</add>
                            </subst></del>
                        <add>Schmuck</add>
                    </subst></l>
                <l n="9649">Klatſchender Wärterinnen <subst>
                        <del>Zunft</del>
                        <add>Schaar</add>
                    </subst></l>
                <l n="9650"><subst>
                        <del>Unvernünftiges Treiben</del>
                        <add>Unvernünftigen Wähnen’s</add>
                    </subst>.</l>
                <l n="9651">Kräftig und <seg f:questionedBy="#ri">zierlich</seg> aber <subst xml:id="ziehtEr">
                        <del><add>schlüpft</add></del>
                        <add>zieht</add>
                    </subst></l>
                <l n="9651"><subst xml:id="Schon-Schalk">
                        <del>Zieht er schon</del>
                        <add>Schon der Schalk</add>
                    </subst><join target="#ziehtEr #Schon-Schalk" result="subst"/> die <subst>
                        <del>beugſamen</del>
                        <add>geſchmeidigen</add>
                    </subst></l>
                <l n="9653"><subst>
                        <del>Dehnsamen</del>
                        <add><alt target="#federkraeftigen #DochElastischen" mode="excl"/>
                            <seg xml:id="federkraeftigen"><del>federkräftigen</del></seg>
                            <seg xml:id="DochElastischen">Doch <subst>
                                    <del>elaſtiſchen</del>
                                    <add>gelenken</add>
                                </subst></seg>
                        </add>
                    </subst> Glieder</l>
                <l n="9654">Liſtig heraus, die purpurne</l>
                <l n="9655"><orig>Ae</orig>ngſtlich <subst f:proposedBy="#ri" xml:space="default">
                        <del>drückende</del>
                        <add><alt target="#beengende #zwaengende" mode="excl"/>
                            <seg xml:id="beengende">beengende</seg>
                            <seg xml:id="zwaengende">zwängende</seg>
                        </add>
                    </subst> <subst f:proposedBy="#ri" xml:space="default">
                        <del>Schaale</del>
                        <add>Hülſe</add>
                    </subst></l>
                <l n="9656">Laſſend <add>ruhig</add> an seiner Statt:</l>
                <l n="9657">
                    <subst>
                        <del>So wie der Schmetterling</del>
                        <add>Gleich dem <alt target="#behenden #umgestalteten #fertigen" mode="excl"/>
                            <seg xml:id="behenden"><del change="#verband_l_9657-9659">behenden</del></seg>
                            <seg xml:id="umgestalteten"><del>umgeſtalteten</del></seg>
                            <seg xml:id="fertigen">fertigen</seg> Schmetterling</add>
                    </subst>
                    <space dim="horizontal" unit="chars" quantity="5"/>
                </l>
                <l n="9658"><subst>
                        <del>Aus <seg f:questionedBy="#ri">dem</seg> ſtarren</del>
                        <add>der aus ſtarrem</add>
                    </subst> Puppenzwang</l>
                <l n="9659">Flügelentfaltend <add change="#verband_l_9657-9659">behendig</add>
                    schlüpft</l>
                <l n="9660"><del><add>In den</add></del> <subst xml:space="default">
                        <del>Sonnedurchſtrahleten</del>
                        <add>Sonnedurchſtrahlten</add>
                    </subst> <add><subst f:revType="soon"><del><gap quantity="5" unit="chars" precision="medium"/></del><add>reinen</add></subst></add> Aether <subst xml:space="default">
                        <del><subst xml:space="default">
                                <del><add><unclear cert="low">himmliſch</unclear></add></del>
                                <add>viel <alt target="#viel1 #viel2" mode="excl"/><seg xml:id="viel1">kühn</seg>
                                    <seg xml:id="viel2">rein</seg></add>
                            </subst></del>
                        <add>kühn</add>
                    </subst></l>
                <l n="9661"><add>und</add> <seg f:questionedBy="#ri">Muth</seg>willig <subst xml:space="default">
                        <del><restore>durchflatternd</restore></del>
                        <add><del f:revType="instant">entflatter</del></add>
                    </subst><!-- unklare Änderung --> <add><unclear cert="low">eigener</unclear> Gaukler</add><!-- unklare Änderung --></l>
            </lg>
            <pb n="02"/>
            <lg type="stanza">
                <head>Antiſtrophe.</head>
                <l n="9662">So auch Er der behendeſte</l>
                <l n="9663">Daſs er schalkiſchen Dieben</l>
                <l n="9664">Vortheil suchenden Allen auch</l>
                <l n="9665">Ewig günſtiger Dæmon sey</l>
                <l n="9666">Dieſs bethätigt er alsobald</l>
                <l n="9667">Durch gewandteſte Künſte.</l>
                <l n="9668">Gleich des Meeresbeherrſcher raubt</l>
                <l n="9669">Er den Trident, ja dem Ares ſelbst</l>
                <l n="9670">Schlau das Schwerdt aus der Scheide.</l>
                <l n="9671">Bogen und Pfeile dem Apoll</l>
                <l n="9672">Wie Hephæſtos die Zange</l>
                <l n="9673">Selber Zeus, des Vaters, Blitz</l>
                <l n="9674">Nähm’ er, schreckt ihn das Feuer nicht</l>
                <l n="9675">Doch dem <subst f:proposedBy="#ri">
                        <del>Amor</del>
                        <add>Eros</add>
                    </subst> siegt er ob</l>
                <l n="9676">Im Beinſtellenden Kämpferſpiel,</l>
                <l n="9677">Raubt der ihm koſenden Cypris <subst f:proposedBy="#ri">
                        <del>gar</del>
                        <add>selbst</add>
                    </subst></l>
                <l n="9678">Noch vom Buſen den Gürtel.</l>
            </lg>
        </body>
